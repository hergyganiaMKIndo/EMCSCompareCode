<div class="modal fade" id="modalUpdateStatusForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
     aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header text-center">
                <h4 class="modal-title w-100 font-weight-bold">Form Update Status</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body mx-3">
                <div class="md-form mb-5">
                    <label>No Item</label>
                    <input type="text" id="orangeForm-name" class="form-control validate">
                </div>
                <div class="md-form mb-5">
                    <label>Item Name</label>
                    <input type="text" id="itemName" class="form-control validate">

                </div>

                <div class="md-form mb-4">
                    <label>PR No</label>
                    <input type="text" id="prNo" class="form-control validate">
                </div>

                <div class="md-form mb-4">
                    <label>Status</label>
                    <select name="status" id="status" class="form-control validate">
                        <option value="this">On Schedule</option>
                        <option value="this">Risk Delay</option>
                        <option value="this">Delay</option>
                        <option value="this">POD</option>
                        <option value="this">BAST</option>
                    </select>
                </div>
                <div class="md-form mb-4">
                    <label>Apply Status For</label>
                    <select name="status" id="status" class="form-control validate">
                        <option value="this">This Item</option>
                        <option value="this">PO</option>
                        <option value="this">PR</option>
                        <option value="this">ALL</option>
                    </select>
                </div>
                <div class="md-form mb-4">
                    <label>Notes</label>
                    <input type="text" id="Notes" placeholder="N/A" class="form-control validate">
                </div>

            </div>
            <div class="modal-footer d-flex justify-content-center">
                <button class="btn btn-primary">Save</button>
            </div>
        </div>
    </div>
</div>


