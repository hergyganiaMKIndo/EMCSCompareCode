@{

    ViewBag.Title = "Master Vendor";
    Layout = "~/Views/Post/_Layout.cshtml";
}
<div class="row" style="margin-top:75px;">
    <div class="custom-grid-style">
        <button class="btn btn-default" data-last="~/HomeVendor" onclick="goToLastPage()" type="button"><i class="fa fa-chevron-circle-left"></i>&nbsp;Back to List</button>&nbsp;
    </div>
</div>
<div class="row">
    <div style="margin-top:10px;">
        <div style="margin-top:20px">
            <div id="table-inprogress" class="table-responsive">
                <button class="btn btn-success float-left" style="margin-bottom:5px;" type="button" onclick="initModalCreateVendor()"><i class="fa fa-plus"></i>&nbsp;Add New</button>&nbsp;
                <table id="table-vendor" data-classes="table-borderless" class="table table-hover table-striped" style="width:100%;"></table>
            </div>
        </div>
    </div>
</div>

@Html.Partial("~/Views/Post/ModalVendor.cshtml")

