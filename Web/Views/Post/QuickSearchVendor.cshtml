<link href="~/Content/POST/bootstrap-table.min.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap-table.min.js"></script>
@section scripts{
    <script>
        var selected = "PR Number";
        function setValueTitle(name) {
            $("#search-by-text").html(name);
            selected = name;
            if (name === "PR Number") {
                $("#result-none").show();
                $("#result-multiple").hide();
                $("#result-single").hide();
            } else if (name === "PO Number") {
                $("#result-single").show();
                $("#result-none").hide();
                $("#result-multiple").hide();
            } else if (name === "PO Date") {
                $("#result-single").hide();
                $("#result-none").hide();
                $("#result-multiple").show();
            } else {
                $("#result-none").show();
                $("#result-single").hide();
                $("#result-multiple").hide();
            }
        }

        $("#table-po").bootstrapTable();

        var $tableInprogress = $('#table-inprogress').bootstrapTable({
            detailView: true,
            onExpandRow: function (index, row, e) {
                /* eslint no-use-before-define: ["error", { "functions": false }]*/
                var childTable = $("#child-table-inprogress").html();
                e.html(childTable);
            }
        })

        var $tableInprogress = $('#table-done').bootstrapTable({
            detailView: true,
            onExpandRow: function (index, row, e) {
                /* eslint no-use-before-define: ["error", { "functions": false }]*/
                var childTable = $("#child-table-done").html();
                e.html(childTable);
            }
        })
    </script>
}
