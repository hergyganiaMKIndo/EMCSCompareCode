@{
    ViewBag.Title = "Milestone";
}


<div class="row">
    <div class="col-md-12">
        <div class="box box-warning">
            <div id="parent">
                <form role="form">
                    <div class="box-body">
                        <div class="row">

                            <div id="AdvancePage">
                                <div class="col-lg-12" style="padding-top:25px;">
                                    <!-- FILTER KOLOM KIRI - START -->
                                    <div class="col-lg-6" style="padding-left:120px !important;">
                                        <div class="form-group">
                                            <label class="control-label col-lg-4 col-xs-4">Serial Number</label>
                                            <div class="input-group col-lg-6 col-xs-5">
                                                <input id="txtSerialNumber" type="text" class="form-control" />
                                            </div>
                                        </div>
                                        <!-- FILTER KOLOM KIRI - END -->
                                    </div>

                                    <!-- FILTER KOLOM KANAN - START -->
                                    <div class="col-lg-6" style="padding-left:100px !important;">
                                        <div class="form-group">
                                            <label class="control-label col-lg-4 col-xs-4">Model</label>
                                            <div class="input-group col-lg-6 col-xs-5">
                                                <input type="text" class="form-control" />
                                            </div>
                                        </div>
                                    </div>
                                    <!-- FILTER KOLOM KANAN - END -->
                                </div>

                            </div>
                            <div class="col-lg-12">
                                <hr>
                                <center>
                                    <table class="table table-bordered table-striped" id="tableDTSMilestone" style="table-layout:fixed;"></table>
                                </center>
                            </div>
                        </div>
                    </div>
                </form>
                <br />
                <div class="progress">
                    <div class="progress-bar"></div>
                </div>

                <div id="status"></div>
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts.app/DeliveryTrackingStatus/DTSMilestone.cshtml.js"></script>
<script src="~/Scripts.app/Report/_report.js"></script>