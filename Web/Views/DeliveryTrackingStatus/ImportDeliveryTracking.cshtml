@{
    ViewBag.Title = "Import Delivery Tracking Status";
}

@using System.Web.Mvc
<script src="~/Scripts/jquery.table2excel.js"></script>

<div class="row">
    <div class="col-md-12">
        <div class="box box-warning">
            <form role="form">
                <div class="box-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="col-lg-6">
                            </div>
                        </div>

                        <div class="col-lg-12">
                            <div class="col-lg-6">
                            </div>
                            <div class="col-lg-6">

                            </div>

                        </div>

                        <div class="col-lg-12">
                            <hr>
                            <center>
                                <div class="toolbar">
                                    <b href="" class="btn btn-info" onclick="document.getElementById('filexls').click(); return false" style="width:150px;">
                                        <i class="fa fa-upload"></i> Import/Upload
                                    </b>
                                    <a href="~/temp/Template_SHIPMENT.XLSX" style="width:150px;" class="btn btn-info">
                                        <i class="fa fa-download"></i> Download Template
                                    </a>
                                    <table class="table table-bordered table-striped" id="tableLogImportDTS" style="table-layout:fixed;"></table>
                                </div>
                            </center>
                        </div>
                    </div>
                </div>

                <br />
                <div class="progress">
                    <div class="progress-bar"></div>
                </div>

                <div id="status"></div>

            </form>

            <form id="submitExcel" method="post" action="ImportDTS" enctype="multipart/form-data">
                <input type="file" id="filexls" name="upload" style="visibility: hidden;" onchange="$('#submitExcel').submit();" />
            </form>

        </div>
    </div>
</div>

<script src="~/Scripts.app/DeliveryTrackingStatus/ImportDTS.cshmtl.js"></script>