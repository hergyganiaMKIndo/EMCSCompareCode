<script type="text/javascript" src="~/Scripts.app/DeliveryTrackingStatus/MilestoneDTS.js"></script>

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <h4 class="modal-title" id="milesoneModalLabel">Milestone Delivery Tracking Status</h4>
</div>

<div class="modal-body">
    <div class="row">
        <form class="form-horizontal">
            <div class="col-lg-12">
                <div class="col-lg-6">
                    <div class="form-group">
                        <label class="control-label col-lg-4 col-xs-4">Sales Order Number</label>
                        <div class="input-group col-lg-8 col-xs-8">
                            <input type="text" id="SN" name="SN" class="form-control" readonly="readonly" value="@ViewBag.SalesOrderNumber" />
                        </div>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <label class="control-label col-lg-4 col-xs-4">Out Bound Delivery</label>
                        <div class="input-group col-lg-8 col-xs-8">
                            <input type="text" id="Model" name="Model" class="form-control" readonly="readonly" value="@ViewBag.NODI" />
                        </div>
                    </div>
                </div>
            </div>
        </form>
        <div class="col-lg-12" style="margin-top:10px;">
            <table class="table table-bordered table-striped" id="tableMilestoneDTS" style="table-layout:fixed;"></table>
        </div>

    </div>

    <div class="nav-tabs-custom nav-justified tabs-style-underline">
        <ul class="nav nav-justified nav-tabs "></ul>
        <!-- /.tab-content -->
    </div>
</div>

<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        iniTableMilestineDTS('@ViewBag.NODI', '@ViewBag.SalesOrderNumber');
    });
</script>