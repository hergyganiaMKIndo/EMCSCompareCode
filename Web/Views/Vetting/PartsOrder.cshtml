@{Layout = "";}
@model  App.Web.Models.PartsOrderView

<div class="row" style="margin-right:2px">
	<div class="row">
		<!-- FILTER KOLOM KIRI - START -->
		<div class="col-lg-6">
			<div class="clearfix"></div>
			<div class="form-group">
				<label class="control-label col-lg-4 col-xs-4">Invoice No</label>
				<div class="input-group col-lg-8 col-xs-8">
					<input id="InvoiceNo" type="text" class="form-control" />
				</div>
			</div>

			<div class="form-group">
				<label class="control-label col-lg-4 col-xs-4">Agreement Type</label>
				<div class="input-group col-lg-8 col-xs-8">
					@*<input id="AgreementType" type="text" class="form-control" />*@
					@Html.ListBoxFor(model => model.selAgreementType, new SelectList(Enumerable.Empty<SelectListItem>()), new { id = "selAgreementType", @class = " input-sm form-control", @style = "width:95%;display:none" })
				</div>
			</div>

			<div class="form-group">
				<label class="control-label col-lg-4 col-xs-4">J Code</label>
				<div class="input-group col-lg-8 col-xs-8">
					@Html.ListBoxFor(model => model.selJCode, new SelectList(Enumerable.Empty<SelectListItem>()), new { id = "selJCode", @class = " input-sm form-control", @style = "width:95%;display:none" })
				</div>
			</div>

			<!-- FILTER KOLOM KIRI - END -->
		</div>

		<!-- FILTER KOLOM KANAN - START -->
		<div class="col-lg-6">
			<div class="form-group">
				<label class="control-label col-lg-4 col-xs-4">Invoice Date</label>
				<div class="input-group col-lg-8 col-xs-8">
					<input id="InvoiceDate" type="text" class="form-control pull-right daterange" />
					<div class="input-group-addon cal">
						<i class="fa fa-calendar"></i>
					</div>
				</div>
			</div>

			<div class="form-group">
				<label class="control-label col-lg-4 col-xs-4">Store Number</label>
				<div class="input-group col-lg-8 col-xs-8">
					<input id="StoreNumber" type="text" class="form-control" />
				</div>
			</div>

			@*<div class="form-group">
				<label class="control-label col-lg-4 col-xs-4">DA Number</label>
				<div class="input-group col-lg-8 col-xs-8">
					<input id="DANumber" type="text" class="form-control" />
				</div>
			</div>*@

		</div>
		<!-- FILTER KOLOM KANAN - END -->

	</div>
	@*<div class="col-lg-12" style="text-align:right;padding-right:30px;">
				<button id="btnFilter" type="button" class="btn btn-success" style="width:125px;"><i class="fa fa-search"></i> Filter</button>
				<button type="button" id="btn-clear" class="btn btn-danger" style="width:125px;"><i class="fa fa-times-circle"></i> Clear</button>
		</div>*@
</div>

<div class="row" style="text-align:right;margin-right:4px">
	<button id="btnFilter" type="button" class="btn btn-success" style="width:125px;"><i class="fa fa-search"></i> Filter</button>
	<button type="button" id="btn-clear" class="btn btn-danger" style="width:125px;"><i class="fa fa-times-circle"></i> Clear</button>
</div>

<div class="row" style="margin-top:9px;margin-right:2px">
	<div style="border-top: 1px solid #e3e3e3;"></div>
</div>

<div class="row" style="margin-right:2px">
	<div class="toolbar">
		<a href="#" class="btn btn-warning downloadExcel">
					<i class="fa fa-download"></i> Download to Excel
			</a>
	</div>
	<table class="table table-bordered table2excel" id="tablePartsOrder"></table>
</div>

<script>
	var vettingRoute = "@Model.VettingRoute";
</script>
