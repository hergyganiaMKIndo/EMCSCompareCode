@Styles.Render("~/Content/EMCS/Custom.css")
@Styles.Render("~/Content/EMCS/dropzone.css")
@Styles.Render("~/Content/EMCS/SweetAlert/sweetalert2.min.css")
<div class="row">
    <div class="col-xs-12 table-responsive">
        <h3>Form : @ViewBag.FormType , FormNumber : @ViewBag.FormNumber</h3>
        @if (ViewBag.FormType != "BlAwb")
        {
            <table class="table" data-toolbar=".toolbarCipl" id="tableRequestForChangeDetail"></table>
        }
        <input type="hidden" id="RFCId" value="@ViewBag.RFCId" />
        <input type="hidden" id="formId" value="@ViewBag.FormId" />
        <input type="hidden" id="FormType" value="@ViewBag.FormType" />

        <div class="row">
            @if (@ViewBag.FormType == "CIPL")
            {
                <div>
                    <div class="col-lg-12">
                        <div class="">
                            <div class="fa-pull-left" style="padding-right:2px">
                                <h4>Original items</h4>
                            </div>
                        </div>
                        <table class="table table-bordered" data-toolbar=".toolbarParts" id="tablepartCipl"></table>
                    </div>
                </div>
            }

            @if (@ViewBag.FormType == "CIPL")
            {
                <div>
                    <div class="col-lg-12">
                        <div class="">
                            <div class="fa-pull-left" style="padding-right:2px">
                                <h4>Requested change</h4>
                            </div>
                        </div>
                        <table class="table table-bordered" data-toolbar=".toolbarEmail" id="tableRequestForChangeList"></table>
                    </div>
                </div>
            }
            @if (@ViewBag.FormType == "Cargo")
            {
                <div>
                    <div class="col-lg-12">
                        <div class="">
                            <div class="fa-pull-left" style="padding-right:2px">
                                <h4>Original items</h4>
                            </div>
                        </div>
                        <table class="table table-bordered" id="tableCargoForm" data-maintain-selected="true" data-unique-id="Id" data-checkbox="true"></table>
                    </div>
                </div>
            }

            @if (@ViewBag.FormType == "Cargo")
            {
                <div>
                    <div class="col-lg-12">
                        <div class="">
                            <div class="fa-pull-left" style="padding-right:2px">
                                <h4>Requested change</h4>
                            </div>
                        </div>
                        <table class="table table-bordered" id="tableCargoItemHistory" data-maintain-selected="true" data-unique-id="Id" data-checkbox="true"></table>
                    </div>
                </div>
            }
            @if (@ViewBag.FormType == "GoodsReceive")
            {
                <div>
                    <div class="col-lg-12">
                        <div class="">
                            <div class="fa-pull-left" style="padding-right:2px">
                                <h4>Original items</h4>
                            </div>
                        </div>
                        <table class="table table-bordered" id="tablearmada" data-maintain-selected="true" data-unique-id="Id" data-checkbox="true"></table>
                    </div>
                </div>
            }

            @if (@ViewBag.FormType == "GoodsReceive")
            {
                <div>
                    <div class="col-lg-12">
                        <div class="">
                            <div class="fa-pull-left" style="padding-right:2px">
                                <h4>Requested change</h4>
                            </div>
                        </div>
                        <table class="table table-bordered" id="tablearmadaForRFC" data-maintain-selected="true" data-unique-id="Id" data-checkbox="true"></table>
                    </div>
                </div>

            }
            @if (@ViewBag.FormType == "BlAwb")
            {
                <div>
                    <div class="col-lg-12">
                        <div class="">
                            <div class="fa-pull-left" style="padding-right:2px">
                                <h4>Original items</h4>
                            </div>
                        </div>
                        <table class="table table-bordered" id="tableBlAwbItems" data-maintain-selected="true" data-unique-id="Id" data-checkbox="true"></table>
                    </div>
                </div>
            }

            @if (@ViewBag.FormType == "BlAwb")
            {
                <div>
                    <div class="col-lg-12">
                        <div class="">
                            <div class="fa-pull-left" style="padding-right:2px">
                                <h4>Requested change</h4>
                            </div>
                        </div>
                        <table class="table table-bordered" id="tableBlAwbItemForRFC" data-maintain-selected="true" data-unique-id="Id" data-checkbox="true"></table>
                    </div>
                </div>

            }
        </div>

    </div>

    <div id='myModalUploadPreview' class='modal fade in'>
        <div class="modal-dialog" style="width:800px">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title"><i class="fa fa-edit"></i>&nbsp;Preview File Document</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group hidden">
                        <label class="control-label col-md-3 col-xs-12">IdDocument</label>
                        <div class="input-group col-md-9 col-xs-12">
                            <input type="text" class="form-control" id="IdDocument" name="IdDocument">
                        </div>
                    </div>
                    <div class="form-group hidden">
                        <label class="control-label col-md-3 col-xs-12">Filename</label>
                        <div class="input-group col-md-9 col-xs-12">
                            <input type="text" class="form-control" id="Filename" name="Filename">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <iframe title="File Name Cipl File" id="framePreview" name="framePreview" src="" style="width:100%;height:800px;"></iframe>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xs-12">
        <p><b>Reason :- </b></p><p><b id="changehistory"></b></p>

        <div class="col-md-6 pull-right">
            <div class="form-group pull-right" style="margin-top:20px">
                <div class="" role="group">
                    <button type="button" class="btn btn-success" id="btnApproveChangeHistory"><i class="fa fa-paper-plane"></i> Approve change</button>
                    <button type="button" class="btn btn-danger" id="btnRejectChangeHistory"><i class="fa fa-times"></i> Reject</button>
                    <a href="/emcs/mytask" class="btn btn-primary"><i class="fa fa-times"></i> Cancel</a>
                </div>
            </div>
        </div>

    </div>
</div>
@section scripts
{
    <script src="@Url.Content("~/Scripts.app/EMCS/function.cshtml.js")"></script>
    <script src="@Url.Content("~/Content/EMCS/SweetAlert/sweetalert2.min.js")"></script>
    <script src="@Url.Content("~/Scripts.app/EMCS/Custom.js")"></script>
    <script src="@Url.Content("~/Content/EMCS/DropZone/dropzone.js")"></script>
    <script src="@Url.Content("~/Scripts.app/EMCS/mytask/TaskRequestForChangeDetail.cshtml.js")"></script>
    <script src="@Url.Content("~/Scripts.app/EMCS/Custom.js")"></script>
}