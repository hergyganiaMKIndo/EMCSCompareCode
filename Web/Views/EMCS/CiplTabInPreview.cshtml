            {
                @Html.Partial("~/Views/EMCS/CiplPreviewInvoice/InvoiceUe.cshtml")
            }
            else if (Model.Data.Category == "MISCELLANEOUS")
            {
                @Html.Partial("~/Views/EMCS/CiplPreviewInvoice/InvoiceMisc.cshtml")
            }
            else if (Model.Data.Category == "CATERPILLAR SPAREPARTS")
