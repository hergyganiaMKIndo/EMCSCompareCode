<table class="table table-bordered table-striped" data-toolbar=".toolbarDelegation" id="tbl-task-delegation"></table>

<div id="ModalBL" class="modal fade" role="dialog">
    <div class="modal-dialog modal-md">

        <div class="modal-content" id="modal-blawb">
            <div class="modal-header mytask">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Upload Document BL/AWB</h4>
            </div>
            <form id="FormUploadBlAwb">
                <div class="modal-body">
                    <div class="form-group hide">
                        <label class="control-label col-lg-4 col-xs-4">ID</label>
                        <div class="input-group col-lg-8 col-xs-8">
                            <input type="text" class="form-control" id="idCL" name="idCL" disabled>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-lg-4 col-xs-4">
                            BL/AWB No
                        </label>
                        <div class="input-group col-lg-8 col-xs-8">
                            <input type="text" class="form-control" id="blAWbNo" name="blAWbNo" placeholder="No">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-lg-4 col-xs-4">Date</label>
                        <div class="input-group col-lg-8 col-xs-8">
                            <input type="text" class="form-control date" id="datelLAwb" name="datelLAwb" placeholder="Date">
                            <div class="input-group-addon cal">
                                <i class="fa fa-calendar"></i>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-lg-4 col-xs-4">Document Required</label>
                        <div class="input-group col-lg-8 col-xs-8">
                            <div id="UploadDocumentBlAwb" class="dropzone"></div>
                        </div>
                    </div>

                </div>
            </form>
            <div class="modal-footer">
                <button id="SaveDocumentBlAwb" type="button" class="btn btn-primary">Save</button>
            </div>
        </div>

    </div>
</div>
