<style>
    table tr td, th {
        background: white;
    }

    .border_title_left {
        border-left: 1px solid #c6c2c2;
    }

    .border_title {
        padding-left: 10px;
        border-bottom: 1px solid #c6c2c2;
        border-right: 1px solid #c6c2c2;
        width: 250px;
    }

    label.custom {
        margin-bottom: 0px !important;
        vertical-align: top;
        display: inline-block;
        font-weight: 100;
    }

    label.st {
        width: 100px;
    }

    label.nd {
        width: 5px;
    }

    label.th {
        width: 250px;
    }

    label.custom.total.st {
        font-weight: 900;
        width: 190px;
    }

    label.custom.total.nd {
        font-weight: 900;
    }

    label.custom.total.th {
        width: 190px;
        text-align: right;
    }

    .pl-pl-8 {
        padding-left: 8px !important;
        padding-right: 8px !important;
    }
</style>

<div class="row" style="margin-top:15px;margin-bottom:15px;padding-left:139px">
    <div class="col-md-12">
        <strong>
            <a class="btn btn-info" href="~/download/DownloadPl/@Model.Data.Id"><i class="fa fa-file-pdf-o"></i> Export Packing List</a>
        </strong>
    </div>
</div>

<div id="preview" style="overflow-y: auto;">
    <object style="width: 100%; height: 29.7cm;" data="~/download/DownloadPl/@Model.Data.Id"
            type="application/pdf">
        Gagal memuat dokumen
    </object>
</div>

