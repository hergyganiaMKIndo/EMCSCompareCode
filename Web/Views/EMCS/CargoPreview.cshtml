<style>
    table tr td, th {
        background: white;
    }

    .border_title_left {
        border-left: 1px solid #c6c2c2;
    }

    .border_title {
        padding-left: 10px;
        border-bottom: 1px solid #c6c2c2;
        border-right: 1px solid #c6c2c2;
        width: 250px;
    }

    label.custom {
        margin-bottom: 0px !important;
        vertical-align: top;
        display: inline-block;
        font-weight: 100;
    }

    label.st {
        width: 100px;
    }

    label.nd {
        width: 5px;
    }

    label.th {
        width: 250px;
    }

    label.custom.total.st {
        font-weight: 900;
        width: 190px;
    }

    label.custom.total.nd {
        font-weight: 900;
    }

    label.custom.total.th {
        width: 190px;
        text-align: right;
    }

    label.custom.total.single {
        width: 400px;
        text-align: right;
    }
</style>
<div style="width:950px !important;border:1px solid;border-color:darkgray;padding:10px 10px 10px 10px;margin:20px auto">
