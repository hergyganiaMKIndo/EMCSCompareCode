            <tr>
                <td class="border_left v_top" colspan="4">
                    <label class="custom single">@Model.TemplateHeader.ConsigneeAddress</label>
                </td>
                <td></td>
                <td class="v_top border_right" colspan="4">
                    <label class="custom single">@((Model.TemplateHeader.NotifyName == "-") ? Model.TemplateHeader.SoldToAddress : Model.TemplateHeader.NotifyAddress)</label>
                </td>
            </tr>
            <tr>
                <td class="border_left v_top" colspan="4">
                    <label class="custom single">@Model.TemplateHeader.ConsigneePic</label>
                </td>
                <td></td>
                <td class="v_top border_right" colspan="4">
                    <label class="custom single">@((Model.TemplateHeader.NotifyName == "-") ? Model.TemplateHeader.SoldToPic : Model.TemplateHeader.NotifyPic)</label>
                </td>
            </tr>
            <tr>
                <td class="v_top border_left" colspan="4">
                    <label class="custom single">@Model.TemplateHeader.ConsigneeEmail</label>
                </td>
                <td></td>
                <td class="v_top border_right" colspan="4">
                    <label class="custom single">@((Model.TemplateHeader.NotifyName == "-") ? Model.TemplateHeader.SoldToEmail : Model.TemplateHeader.NotifyEmail)</label>
                </td>
            </tr>
            <tr><td colspan="9">&nbsp;</td></tr>
        </table>

        <div>&nbsp;</div>
        <table class="">
            <tr>
                <td class="text_bold border_top border_left" colspan="4" style="width:45%;">
                    <div class="border_title" style="border-bottom:none;">CONTAINER CONTENTS LIST</div>
                </td>
                <td class="" style="width:10%;">&nbsp;</td>
                <td class="" colspan="4" style="width:45%;">&nbsp;</td>
            </tr>
        </table>

        @{
            if (Model.Data.CargoType == "FCL")
            {
                <table border="0" class="table-data">
                    <thead>
                        <tr>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:3%;height:2em">NO</th>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:10%">CONTAINER NUMBER</th>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:7%">SEAL #</th>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:10%">CONTAINER TYPE</th>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:10%">NUMBER & KIND OF PACKAGES</th>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:10%">CASE NUMBER</th>
                            <th class="text_center tabledata_spacing border_bottom" colspan="2" style="width:10%">REFERENCE</th>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:10%">CARGO DESCRIPTION</th>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:10%">NET WEIGHT</th>
                            <th class="text_center tabledata_spacing border_bottom border_right" rowspan="2" style="width:10%">GROSS WEIGHT</th>
                        </tr>
                        <tr>
                            <th class="text_center text_bold tabledata_spacing border_bottom" style="width:10%;background-color:white">EDI Number #</th>
                            <th class="text_center text_bold tabledata_spacing border_bottom" style="width:10%;background-color:white">IN BOUND-DA #</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.TemplateDetail)
                        {

                            <tr>
                                <td class="tabledata_spacing text_right">@item.ItemNo</td>
                                <td class="tabledata_spacing">@item.ContainerNumber</td>
                                <td class="tabledata_spacing">@item.SealNumber</td>
                                <td class="tabledata_spacing">@item.ContainerType</td>
                                <td class="tabledata_spacing">@item.TotalCaseNumber</td>
                                <td class="tabledata_spacing">@item.CaseNumber</td>
                                <td class="tabledata_spacing">@item.Do</td>
                                <td class="tabledata_spacing">@item.InBoundDa</td>
                                <td class="tabledata_spacing">@item.Description</td>
                                <td class="tabledata_spacing text_right">@string.Format("{0:C}", @item.NetWeight)</td>
                                <td class="tabledata_spacing text_right border_right">@item.GrossWeight</td>
                            </tr>
                        }
                        <tr class="spacing">
                            <td colspan="11" class="tabledata_spacing"></td>
                        </tr>
                    </tbody>
                </table>
            }
            else
            {
                <table border="0" class="table-data">
                    <thead>
                        <tr>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:3%;height:2em;vertical-align: top;">NO</th>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:10%;vertical-align: top;">NUMBER & KIND OF PACKAGES</th>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:7%;vertical-align: top;">CASE NUMBER</th>
                            <th class="text_center tabledata_spacing border_bottom" colspan="2" style="width:10%;vertical-align: top;">REFERENCE</th>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:15%;vertical-align: top;">CARGO DESCRIPTION</th>
                            <th class="text_center tabledata_spacing border_bottom" rowspan="2" style="width:8%;vertical-align: top;">NET WEIGHT</th>
                            <th class="text_center tabledata_spacing border_bottom border_right" rowspan="2" style="width:8%;vertical-align: top;">GROSS WEIGHT</th>
                        </tr>
                        <tr>
                            <th class="text_center tabledata_spacing border_bottom" style="width:10%;background-color:white;vertical-align: top;">EDI Number #</th>
                            <th class="text_center tabledata_spacing border_bottom" style="width:10%;background-color:white;vertical-align: top;">IN BOUND-DA #</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.TemplateDetail)
                        {
                            <tr>
                                <td class="tabledata_spacing text_center">@item.ItemNo</td>
                                <td class="tabledata_spacing text_center">@item.TotalCaseNumber</td>
                                <td class="tabledata_spacing text_center">@item.CaseNumber</td>
                                <td class="tabledata_spacing text_center">@item.Do</td>
                                <td class="tabledata_spacing text_center">@item.InBoundDa</td>
                                <td class="tabledata_spacing">@item.Description</td>
                                <td class="tabledata_spacing text_right">@string.Format("{0:C}", @item.NetWeight)</td>
                                <td class="tabledata_spacing text_right border_right">@string.Format("{0:C}", @item.GrossWeight)</td>
                            </tr>
                        }
                        @{
                            if (Model.TemplateDetail.Count <= 2)
                            {
                                int dummy = 5;
                                for (int no = 1; no <= dummy; no++)
                                {
                                    <tr><td colspan="9" class="border_left border_right" style="border-top:1px solid white;">&nbsp;</td></tr>
                                    if (no == 5)
                                    {
                                        <tr><td colspan="9" class="border_bottom">&nbsp;</td></tr>
                                    }
                                }
                            }
                        }
                    </tbody>
                </table>
            }
        }
        <div>&nbsp;</div>

        <table class="table-document">
            <tr>
                <td colspan="9"></td>
                @if (Model.Data.TotalPackageBy == "CaseNo")
                {
                    <td colspan="3">TOTAL PACKAGE BY CASENUMBER</td>
                    <td>:</td>
                    <td class="text_bold text_right">@Model.TemplateHeader.TotalCaseNumber</td>
                }
                else
                {
                    <td colspan="3">TOTAL PACKAGE BY ITEM</td>
                    <td>:</td>
                    <td class="text_bold text_right">@Model.TemplateHeader.TotalCaseNumber</td>
                }
            </tr>
            <tr>
                <td colspan="9"></td>
                <td colspan="3">TOTAL NET WEIGHT (KGS)</td>
                <td>:</td>
                <td class="text_bold text_right">@Model.TemplateHeader.TotalNetWeight</td>
            </tr>
            <tr>
                <td colspan="9"></td>
                <td colspan="3">TOTAL GROSS WEIGHT (KGS)</td>
                <td>:</td>
                <td class="text_bold text_right">@Model.TemplateHeader.TotalGrossWeight</td>
            </tr>
            <tr>
