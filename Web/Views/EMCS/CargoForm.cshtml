/*    var CargoTypeName = "Full Container Load";*/
    $(document).ready(function () {

        load_data_tabledoc();
        get_cargodocumentlist();
        $('#IsRFC').val(IsRFC);
        //var value = $('#TotalPackage').val();
        //getTotalData(value);
    });
    $('#TotalPackage').on('change', function () {

        var value = $('#TotalPackage').val();
        getTotalData(value);
    })
    function getTotalData(value) {
        $('#TotalPackage option[value='+value+']').prop('selected',true)
        var IdCargo = $("#CargoID").val();
        $.get(myApp.fullPath + "EMCS/GetTotalDataCargo?Id=" + IdCargo + "&selectvalue=" + value, function (resp) {
            var data = {};
            $.map(resp.data, function (value, key) {
                data[value.Key] = value.Value;
            });
            if (value == "CaseNo") {
                $("#getcount").val(formatCurrency(data.totalPackage, ".", ",", 2));

            }
            else {
                $("#getcount").val(formatCurrency(data.totalPackage, ".", ",", 2));

            }
            $("#inpTotalGrossWeightCl").val(formatCurrency(data.totalGrossWeight, ".", ",", 2));
            $("#inpTotalNetWeightCl").val(formatCurrency(data.totalNetWeight, ".", ",", 2));
            $("#inpTotalVolumeCl").val(data.totalVolume);
        });

    }
</script>
