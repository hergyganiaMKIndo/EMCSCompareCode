@{
    ViewBag.Title = "Create Outbound Shipment";
}

@using System.Web.Mvc

<style type="text/css">
    .form-horizontal .control-label {
        text-align: left !important;
    }

    .error {
        display: none;
        color: red;
        font-weight: bold;
    }

    label.error {
        border: none;
        display: none !important;
    }
    .btn-form {
        width: 100px;
    }
</style>

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <h4 class="modal-title">Requisition Form Approval</h4>
</div>
<div class="modal-body">
    <form class="form" id="formRequest">
        <div class="row">
            <div class="col-lg-12">
                <div class="col-lg-4">
                    <div class="form-group">
                        <label for="ID" class="control-label col-md-4 col-xs-12">ID</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" id="KeyCustom" disabled name="KeyCustom" placeholder="ID" />
                            <input class="hidden" type="hidden" name="formType" placeholder="Form Type" value="U" />
                            <input class="hidden" type="hidden" name="ID" placeholder="ID" value="0" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Origin" class="control-label col-md-4 col-xs-12">Origin</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" id="Origin" disabled name="Origin" placeholder="Origin" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Model" class="control-label col-md-4 col-xs-12">Model</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" disabled id="Model" name="Model" placeholder="Model" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="SerialNumber" class="control-label col-md-4 col-xs-12">Serial Number</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" disabled id="SerialNumber" name="SerialNumber" placeholder="SerialNumber" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Batch" class="control-label col-md-4 col-xs-12">Batch</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" disabled id="Batch" name="Batch" required placeholder="Batch" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="ReqID" class="control-label col-md-4 col-xs-12">Requestor Name</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input id="RequestID" required name="ReqID" class="form-control" disabled placeholder="Requestor Name">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="ReqHp" class="control-label col-md-4 col-xs-12">Requestor HP</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" disabled required id="RequestHP" name="ReqHp" placeholder="Requestor Handphone" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="CustID" class="control-label col-md-4 col-xs-12">Customer Name</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input id="CustomerID" required disabled name="CustID" class="form-control" placeholder="Customer Name" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="CustAddress" class="control-label col-md-4 col-xs-12">Customer Address</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <textarea name="CustAddress" id="CustomerAddress" class="form-control" disabled required placeholder="Customer Address"></textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Kecamatan" class="control-label col-md-4 col-xs-12">Kecamatan</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" disabled id="Kecamatan" name="Kecamatan" required placeholder="kecamatan" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Kabupaten" class="control-label col-md-4 col-xs-12">Kabupaten</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" disabled id="Kabupaten" name="Kabupaten" required placeholder="Kabupaten" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Province" class="control-label col-md-4 col-xs-12">Province</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" disabled id="Province" name="Province" required placeholder="Province" />
                        </div>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="form-group">
                        <label for="PicName" class="control-label col-md-4 col-xs-12">PIC Name</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" disabled id="PicName" name="PicName" required placeholder="PIC Name" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="PicHP" class="control-label col-md-4 col-xs-12">PIC HP</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" disabled name="PicHP" required placeholder="PIC Handphone" />
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="TOD" class="control-label col-md-4 col-xs-12">Term Of Delivery</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <div class="checkbox">
                                <label><input name="TOD" class="TOD" disabled required value="DOOR TO DOOR (DTD)" type="checkbox" /> DOOR TO DOOR (DTD)</label><br />
                                <label><input name="TOD" class="TOD" disabled required value="PORT TO PORT (PTP)" type="checkbox" /> PORT TO PORT (PTP)</label><br />
                                <label><input name="TOD" class="TOD" disabled required value="DOOR TO PORT OR PORT TO DOOR (DTP OR PTD)" type="checkbox" /> DOOR TO PORT OR PORT TO DOOR (DTP OR PTD)</label><br />
                                <label><input name="TOD" class="TOD" disabled required value="CONTAINER YARD  TO CONTAINER YARD ( CY TO CY )" type="checkbox" /> CONTAINER YARD  TO CONTAINER YARD ( CY TO CY )</label><br />
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="SOD" class="control-label col-md-4 col-xs-12">Supporting Of Delivery</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <div class="checkbox">
                                <label><input name="SOD" disabled class="SOD" required value="OPERATOR LOADING & UNLOADING" type="checkbox" /> OPERATOR LOADING & UNLOADING</label><br />
                                <label><input name="SOD" disabled class="SOD" required value="LIFTING TOOLS" type="checkbox" /> LIFTING TOOLS</label><br />
                                <label><input name="SOD" disabled class="SOD" required value="ESCORTED BY POLICE" type="checkbox" /> ESCORTED BY POLICE</label><br />
                                <label><input name="SOD" disabled class="SOD" required value="ESCORTED BY MECHANIC" type="checkbox" /> ESCORTED BY MECHANIC</label><br />
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="INCT" class="control-label col-md-4 col-xs-12">Incoterm</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <div class="checkbox">
                                <label><input type="checkbox" disabled class="INCT" required name="INCT" value="FOT ( FREE ON TRUCK )" /> FOT ( FREE ON TRUCK )</label></br>
                                <label><input type="checkbox" disabled class="INCT" required name="INCT" value="ON PONDATION" /> ON PONDATION</label></br>
                                <label><input type="checkbox" disabled class="INCT" required name="INCT" value="FOB ( FREE ON BOARD )" /> FOB ( FREE ON BOARD )</label></br>
                                <label><input type="checkbox" disabled class="INCT" required name="INCT" value="ON GROUND" /> ON GROUND</label></br>
                                <label><input type="checkbox" disabled class="INCT" required name="INCT" value="FAS ( FREE ALONGSIDE SHIP)" /> FAS ( FREE ALONGSIDE SHIP)</label></br>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="col-lg-4">
                    <div class="form-group">
                        <label for="ExpectedTimeLoading" class="control-label col-md-5 col-xs-12">Expected time Loading</label>
                        <div class="input-group col-md-7 col-xs-12">
                            <input class="form-control date" disabled name="ExpectedTimeLoading" required placeholder="Expected time Loading" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="ExpectedTimeArrival" class="control-label col-md-5 col-xs-12">Expected time Arrival</label>
                        <div class="input-group col-md-7 col-xs-12">
                            <input class="form-control date" disabled name="ExpectedTimeArrival" required placeholder="Expected time Arrival" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="PenaltyLateness" class="control-label col-md-5 col-xs-12">Penalty Lateness</label>
                        <div class="input-group col-md-7 col-xs-12">
                            <div class="checkbox">
                                <label class="checkbox-inline"><input name="PenaltyLateness" disabled required value="true" type="checkbox" /> YES</label>&nbsp;
                                <label class="checkbox-inline"><input name="PenaltyLateness" disabled required value="false" type="checkbox" /> NO</label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="SoNo" class="control-label col-md-5 col-xs-12">SO No</label>
                        <div class="input-group col-md-7 col-xs-12">
                            <input class="form-control" name="SoNo" disabled required placeholder="SO Number" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="DoNo" class="control-label col-md-5 col-xs-12">OD No</label>
                        <div class="input-group col-md-7 col-xs-12">
                            <input class="form-control" name="DoNo" disabled required placeholder="OD Number" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="OdDate" class="control-label col-md-5 col-xs-12">OD Date</label>
                        <div class="input-group col-md-7 col-xs-12">
                            <input class="form-control date" name="OdDate" disabled required placeholder="OD Date" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Unit" class="control-label col-md-5 col-xs-12">Type</label>
                        <div class="input-group col-md-7 col-xs-12">
                            <div class="checkbox">
                                <input type="radio" name="unit" disabled value="MACHINE" checked> MACHINE<br>
                                <input type="radio" name="unit" disabled value="ENGINE"> ENGINE<br>
                                <input type="radio" name="unit" disabled value="FORKLIFT"> FORKLIFT <br />
                                <input type="radio" name="unit" disabled value="RUE"> RUE <br />
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Transportation" class="control-label col-md-4 col-xs-12">Transportation</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <div class="checkbox">
                                <label><input type="checkbox" disabled class="Transportation" required name="Transportation" value="PTTU" /> PTTU </label><br />
                                <label><input type="checkbox" disabled class="Transportation" required name="Transportation" value="Customer" /> Customer</label>
                                
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="DINo" class="control-label col-md-4 col-xs-12">DI Number</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" id="DINo" name="DINo" required placeholder="DI Number" />
                            <div for="DINo" class="error">This field is required</div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Referrence" class="control-label col-md-4 col-xs-12">Referrence</label>
                        <div class="input-group col-md-8 col-xs-12">
                            <input class="form-control" id="Referrence" name="Referrence" required placeholder="Referrence" />
                            <div for="Referrence" class="error">This field is required</div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </form>
</div>
<div class="modal-footer">
    <div class="">
        <button id="Reject" name="Reject" class="processForm btn-form btn btn-warning" type="button"><i class="fa fa-remove"></i> Reject</button>
        <button id="Revise" name="Revise" class="processForm btn-form btn btn-info" type="button"><i class="fa fa-edit"></i> Revise</button>
        <button id="Approve" name="Approve" class="processForm btn-form btn btn-primary" type="button"><i class="fa fa-gear"></i> Approve</button>
        <button id="Complete" name="Complete" class="processForm btn-form btn btn-success" type="button"><i class="fa fa-check-circle"></i> Complete</button>
        <button id="Close" name="Close" class="btn-form btn btn-default" onclick="hideModal();" type="button"><i class="fa fa-remove"></i> Close</button>
    </div>
</div>
@section scripts {
    <script src="/Scripts/loadingoverlay.min.js"></script>
}

