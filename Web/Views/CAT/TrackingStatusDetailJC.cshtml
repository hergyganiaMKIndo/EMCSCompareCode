<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <h4 class="modal-title" id="myModalLabel">Status Job Completed</h4>
</div>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="modal-body">
        <div class="col-lg-12">
            <!-- Default input -->
            <div class="form-horizontal">
                <div class="col-lg-12">
                    <div class="form-group">
                        <label class="control-label col-md-4">DA Number</label>
                        <div class="col-md-8">
                            <input name="DANumber" id="DANumber" class="form-control" value="@ViewBag.DANumber" disabled="disabled" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-4">CRC Completion Date</label>
                        <div class="col-md-8">
                            <input name="CRCCompletionDate" id="CRCCompletionDate" class="form-control" disabled="disabled" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-4">TU Stand ID</label>
                        <div class="col-md-8">
                            <input name="TUStandID" id="TUStandID" class="form-control" value="@ViewBag.TUStandID" disabled="disabled" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-4">TUID</label>
                        <div class="col-md-8">
                            <input name="TUID" id="TUID" class="form-control" value="@ViewBag.TUID" disabled="disabled" />
                        </div>
                    </div>
                </div>
            </div>
        </div>



        <div class="nav-tabs-custom nav-justified tabs-style-underline">
            <ul class="nav nav-justified nav-tabs "></ul>
            <!-- /.tab-content -->
        </div>

    </div>


    <div class="modal-footer">

        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>

    </div>
}
<script>
    $(document).ready(function () {
        var dt = dateFormatterCAT('@ViewBag.CRCCompletionDate');
        $('#CRCCompletionDate').val(dt);
    });
</script>