@using System.Web.Mvc
@using App.Web

@model App.Web.Models.CAT.RptOutstandingOldCoreDetailFilter
@{
    ViewBag.Title = "Report Outstanding Old Core Detail";
}

<style type="text/css">
    .form-horizontal .control-label {
        text-align: left !important;
    }
</style>
<div class="row">
    <div class="col-md-12">
        <div class="box box-warning" style="min-height:555px">
            <div id="parent">
                <form id="form" role="form">
                    <div class="box-body">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="col-lg-6">
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">Store</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.DropDownListFor(m => m.StoreID, new SelectList(Model.StoreList, "ID", "Name"), "ALL", new { @class = "js-states form-control", @style = "display:none" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">SOS</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.DropDownListFor(m => m.SOSID, new SelectList(Model.SOSList, "SOS", "SOS"), "ALL", new { @class = "js-states form-control", @style = "display:none" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">Part No.</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.TextBoxFor(m => m.PartNo, new { @class = "form-control" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">KAL</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.TextBoxFor(m => m.KAL, new { @class = "form-control" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">Model</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.TextBoxFor(m => m.Model, new { @class = "form-control" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">Component</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.TextBoxFor(m => m.Component, new { @class = "form-control" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">SN Unit</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.TextBoxFor(m => m.SNUnit, new { @class = "form-control" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">Range Week</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.DropDownListFor(m => m.RangeWeek, new SelectList(Model.RangeWeek, "ID", "TEXT"), new { @id = "DateFilter", @class = "js-states form-control", @style = "display:none" })
                                        </div>
                                    </div>
                                </div>

                                <div class="col-lg-6">
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">Customer</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @*@Html.DropDownListFor(m => m.CustomerID, new SelectList(Model.CustomerList, "CUSTOMER_ID", "CUSTOMERNAME"), "ALL", new { @class = "js-states form-control", @style = "display:none" })*@
                                            @Html.DropDownListFor(m => m.CustomerID, new SelectList(Enumerable.Empty<SelectListItem>()), "", new { @class = "js-states form-control", @style = "display:none" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">Prefix</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.TextBoxFor(m => m.Prefix, new { @class = "form-control" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">Store Supplied Date</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            <input id="EntryDate" type="text" class="form-control pull-right daterange" readonly="readonly" style="background-color: #ffffff" />
                                            <div class="input-group-addon cal" style="background:#00C0EF;border-color:#00ACD6;">
                                                <i class="fa fa-calendar" style="color:#FFFFFF;"></i>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">Reconditioned WO</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.TextBoxFor(m => m.ReconditionedWO, new { @class = "form-control" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">Sales Doc</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.TextBoxFor(m => m.DocC, new { @class = "form-control" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">Return Doc</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.TextBoxFor(m => m.DocR, new { @class = "form-control" })
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-lg-4 col-xs-4">WCSL</label>
                                        <div class="input-group col-lg-8 col-xs-8">
                                            @Html.TextBoxFor(m => m.WCSL, new { @class = "form-control" })
                                        </div>
                                    </div>
                                    
                                </div>
                            </div>
                            <div class="col-lg-12" style="text-align:right;padding-right:30px;">
                                <button type="button" id="btn-filter" class="btn btn-success" style="width:125px;"><i class="fa fa-search"></i> Filter</button>
                                <button type="button" id="btn-clear" class="btn btn-danger" style="width:125px;"><i class="fa fa-times-circle"></i> Clear</button>
                            </div>
                            <div class="col-lg-12">
                                <div style="border-top: 1px solid #e3e3e3; margin-top:7px"></div>
                                <div class="toolbar">
                                    <a href="#" class="btn btn-info downloadExcel" style="width:150px;">
                                        <i class="fa fa-download"></i> Download
                                    </a>
                                </div>
                                <table class="table table-bordered table2excel" id="table-out-oldcore-detail" style="width:1300px;" data-show-filter="true"></table>
                            </div>
                        </div>
                    </div>
                </form>
            </div>

            <div class="box-body">
                <div class="row">
                    <div id="detail" style="display: none"></div>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script src="~/Scripts.app/cat/OutstandingOldCoreDetail.cshtml.js"></script>
    <script src="/Scripts/extension/export/bootstrap-table-export.js"></script>
    <script src="/Scripts/extension/export/tableExport.js"></script>
    <script src="/Scripts/extension/export/jquery.base64.js"></script>
}




