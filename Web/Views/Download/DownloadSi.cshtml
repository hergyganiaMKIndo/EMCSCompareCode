@model  App.Web.Models.EMCS.CargoSiModel
@{


    ViewBag.Title = "DownloadSi";
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Document Download Packing List</title>
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro&display=swap" rel="stylesheet">
    <style type="text/css">
        html body {
            /*font-family: 'Arial Narrow', Arial, sans-serif;*/
            font-family: sans-serif;
            font-size: 11px;
            letter-spacing: 0.67ex;
            font-weight: 100 !important;
            line-height: 18px;
            text-rendering: optimizeSpeed;
        }

        table tr td, th {
            /*font-family: 'Arial Narrow', Arial, sans-serif;*/
            font-family: sans-serif;
            font-size: 11px;
            letter-spacing: 0.67ex;
            line-height: 18px;
            text-rendering: optimizeSpeed;
        }

        .hidden {
            display: none;
        }

        /* TABLE */
        .table thead tr {
            -webkit-box-shadow: 0px 6px 4px -3px rgba(222,222,222,0.72);
            -moz-box-shadow: 0px 6px 4px -3px rgba(222,222,222,0.72);
            box-shadow: 0px 6px 4px -3px rgba(222,222,222,0.72);
        }

            .table thead tr th {
                /*background: -moz-linear-gradient(left, rgba(60,141,188,1) 0%, rgba(60,141,188,0.93) 56%, rgba(61,142,190,0.93) 58%, rgba(86,156,235,0.93) 100%);
            background: -webkit-gradient(left top, right top, color-stop(0%, rgba(60,141,188,1)), color-stop(56%, rgba(60,141,188,0.93)), color-stop(58%, rgba(61,142,190,0.93)), color-stop(100%, rgba(86,156,235,0.93)));*/
                background-color: black;
                /* background: rgba(60,141,188,1); */
                font-weight: 600;
                line-height: 1;
                color: white;
            }

        .table tbody tr td {
            line-height: 1;
        }

        .table-document {
            border: none;
            border-collapse: collapse;
            width: 100%;
        }

            .table-document tr td {
                background: white;
            }

            .table-document tr td {
                line-height: 20px;
            }

        .table-custom {
            border: 0.5px solid darkgrey !important;
            width: 100%;
            border-collapse: collapse;
        }

        .table-data {
            border: 0.5px solid darkgrey !important;
            width: 100%;
            border-collapse: collapse !important;
        }

            .table-data tr td {
                /*border: 0.5px solid darkgrey !important;*/
            }

        .border_top {
            border-top: 0.5px solid darkgrey !important;
        }

        .border_bottom {
            border-bottom: 0.5px solid darkgrey !important;
        }

        .border_left {
            padding-left: 10px !important;
            border-left: 0.5px solid darkgrey !important;
        }

        .border_right {
            padding-right: 10px !important;
            border-right: 0.5px solid darkgrey !important;
        }

        .text_left {
            text-align: left !important;
        }

        .text_right {
            text-align: right !important;
        }

        .text_center {
            text-align: center !important;
        }

        .text_arial {
            font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
        }

        .text_bold {
            font-weight: 800;
        }

        .tabledata_spacing {
            padding-left: 10px;
            padding-right: 10px;
        }

        .text_docno {
            color: darkorange;
        }

        .text_gold {
            color: gold;
        }

        .text_docno_big {
            font-size: 15px;
            color: darkorange;
        }

        .text_20 {
            font-size: 20px !important;
        }

        .text_18 {
            font-size: 18px !important;
        }

        .text_16 {
            font-size: 16px !important;
        }

        .text_14 {
            font-size: 14px !important;
        }

        .text_12 {
            font-size: 12px !important;
        }

        .text_9 {
            font-size: 9px !important;
        }

        .text_8 {
            font-size: 8px !important;
        }

        .text_justify {
            text-align: justify;
        }

        .text_italic {
            font-style: italic;
        }

        .table-title-data {
            color: red;
        }

        .width_50_persen {
            width: 50%
        }

        .padding_left_10 {
            padding-left: 10px !important;
        }

        .padding_right_10 {
            padding-right: 10px !important;
        }

        thead {
            display: table-header-group
        }

        tfoot {
            display: table-row-group
        }

        tr {
            page-break-inside: avoid;
        }

        .table-title-data div {
            border-right: 1px solid #b9b4b4;
            border-bottom: 1px solid #b9b4b4;
            width: 300px;
            padding-left: 14px;
            margin-left: -10px;
            font-weight: 700;
            color: red;
            margin-top: 0px;
            line-height: 35px;
            color: black;
            z-index: 99999;
        }
    </style>
</head>
<body id="header_document" onload="subst()">
    <div style="width:950px !important;darkgray;padding:0px 0px 0px 0px;margin:20px auto">
        <div style="padding-left:4px;padding-right:4px">
            <div style="padding:10px">
                <table class="table-document">

                    <tr>
                        <td colspan="3" class="border_left border_top">To</td>
                        <td class="border_top">:</td>
                        <td colspan="9" class="border_top border_right text_bold">@Model.Header.Forwarder</td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left">Attention</td>
                        <td>:</td>
                        <td colspan="9" class="border_right text_bold">@Model.Header.ForwarderAttention</td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left">Email</td>
                        <td>:</td>
                        <td colspan="9" class="border_right text_bold">@Model.Header.ForwarderEmail</td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left border_bottom">Contact No.</td>
                        <td class="border_bottom">:</td>
                        <td colspan="9" class="border_bottom border_right text_bold">@Model.Header.ForwarderContact</td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left">Shipper</td>
                        <td>:</td>
                        <td colspan="9" class="border_right text_bold">PT Trakindo Utama</td>
                    </tr>
                    <tr>
                        <td colspan="3" rowspan="3" class="border_left"></td>
                        <td></td>
                        <td colspan="9" class="border_right">Gedung TMT 1, Jl. Cilandak KKO No. 1,  LT. 11 – 17 Suite 1101-1701</td>
                    </tr>
                    <tr>
                        <td></td>
                        <td colspan="9" class="border_right">Kel. Cilandak Timur, Kec. Pasar Minggu, Jakarta Selatan - 12560 |INDONESIA</td>
                    </tr>
                    <tr>
                        <td class="border_bottom"></td>
                        <td colspan="9" class="border_bottom border_right">Tel. (+62-21) 29976620 | Fax. (+62-21) 29976612 | www.trakindo.co.id</td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left border_top">Consignee</td>
                        <td>:</td>
                        <td colspan="9" class="border_right text_bold">@Model.Header.ConsigneeName</td>
                    </tr>
                    <tr>
                        <td colspan="4" class="border_left"></td>
                        <td colspan="9" class="border_right">@Model.Header.ConsigneeAddress</td>
                    </tr>
                    <tr>
                        <td colspan="4" class="border_left"></td>
                        <td colspan="9" class="border_right">@Model.Header.ConsigneePic</td>
                    </tr>
                    <tr>
                        <td colspan="4" class="border_left"></td>
                        <td colspan="9" class="border_right">@Model.Header.ConsigneeEmail</td>
                    </tr>
                    <tr>
                        <td colspan="4" class="border_left border_bottom"></td>
                        <td colspan="9" class="border_bottom border_right">@Model.Header.ConsigneeTelephone</td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left">@((Model.Header.ShipDelivery == "Notify") ? "Notify Party" : "Sold To")</td>
                        <td>:</td>
                        <td colspan="9" class="border_right text_bold">@((Model.Header.ShipDelivery == "Notify") ? Model.Header.NotifyName : Model.Header.SoldToName)</td>
                    </tr>
                    <tr>
                        <td colspan="4" class="border_left"></td>
                        <td colspan="9" class="border_right">@((Model.Header.ShipDelivery == "Notify") ? Model.Header.NotifyAddress : Model.Header.SoldToAddress)</td>
                    </tr>
                    <tr>
                        <td colspan="4" class="border_left"></td>
                        <td colspan="9" class="border_right">@((Model.Header.ShipDelivery == "Notify") ? Model.Header.NotifyPic : Model.Header.SoldToPic)</td>
                    </tr>
                    <tr>
                        <td colspan="4" class="border_left"></td>
                        <td colspan="9" class="border_right">@((Model.Header.ShipDelivery == "Notify") ? Model.Header.NotifyEmail : Model.Header.SoldToEmail)</td>
                    </tr>
                    <tr>
                        <td colspan="4" class="border_left border_bottom"></td>
                        <td colspan="9" class="border_bottom border_right">@((Model.Header.ShipDelivery == "Notify") ? Model.Header.NotifyTelephone : Model.Header.SoldToTelephone)</td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left border_bottom">Terms Of Delivery</td>
                        <td class="border_bottom">:</td>
                        <td colspan="9" class="border_bottom border_right text_bold">@Model.Header.IncoTerm</td>
                    </tr>
                    <tr>
                        <td colspan="7" class="border_left border_bottom border_right">Marks & No</td>
                        <td colspan="6" class="border_left border_bottom border_right">Description</td>
                    </tr>
                    <tr>
                        <td colspan="7" class="border_left border_bottom border_right text_bold" style="vertical-align:top">@Model.Header.ShippingMarks</td>
                        <td colspan="6" class="border_right">
                            <table border="0" style="width:100%">
                                <tr><td class="tabledata_spacing border_right text_bold"> @Model.Header.Description</td></tr>
                                @foreach (var item in Model.Item)
                                {
                                    <tr><td class="tabledata_spacing border_right text_left">@item.Name</td></tr>
                                }
                            </table>
                        </td>
                        @*@foreach (var item in Model.Item)
                            {
                                <td class="tabledata_spacing text_left">@item.Name</td>
                            }*@
                    </tr>
                    <tr>
                        <td colspan="5" class="border_left border_right">Total Gross Weight :</td>
                        <td colspan="4" class="border_left border_right">Net Weight : </td>
                        <td colspan="4" class="border_left border_right border_top">Volume :</td>
                    </tr>
                    <tr>
                        <td colspan="2" class="border_left border_bottom text_bold text_right tabledata_spacing">@Model.Header.TotalGrossWeight</td>
                        <td colspan="3" class="border_bottom border_right">KGS</td>
                        <td colspan="2" class="border_left border_bottom text_bold text_right tabledata_spacing">@Model.Header.TotalNetWeight</td>
                        <td colspan="2" class="border_bottom border_right">KGS</td>
                        <td colspan="2" class="border_left border_bottom text_bold text_right tabledata_spacing">@Model.Header.TotalVolume</td>
                        <td colspan="2" class="border_bottom border_right">M3</td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left border_bottom">Booking Number</td>
                        <td class="border_bottom">:</td>
                        <td class="border_bottom text_bold">@Model.Header.BookingNumber</td>
                        <td colspan="2" class="border_bottom"></td>
                        <td colspan="2" class="border_bottom">Booking Date</td>
                        <td class="border_bottom">:</td>
                        <td class="border_bottom text_bold">@Model.Header.BookingDate</td>
                        <td colspan="2" class="border_bottom border_right"></td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left border_bottom">Port of Loading</td>
                        <td class="border_bottom">:</td>
                        <td colspan="3" class="border_bottom text_bold">@Model.Header.PortOfLoading</td>
                        <td colspan="2" class="border_bottom">Port of Discharge</td>
                        <td class="border_bottom">:</td>
                        <td colspan="3" class="border_bottom text_bold border_right">@Model.Header.PortOfDestination</td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left border_bottom">ETD Date</td>
                        <td class="border_bottom">:</td>
                        <td class="border_bottom text_bold">@Model.Header.Etd</td>
                        <td colspan="2" class="border_bottom"></td>
                        <td colspan="2" class="border_bottom">ETA Date</td>
                        <td class="border_bottom">:</td>
                        <td class="border_bottom text_bold">@Model.Header.Eta</td>
                        <td colspan="2" class="border_bottom border_right"></td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left border_bottom">Vessel Voyage</td>
                        <td class="border_bottom">:</td>
                        <td class="border_bottom text_bold">@Model.Header.VesselVoyage</td>
                        <td colspan="2" class="border_bottom"></td>
                        <td colspan="2" class="border_bottom">Connecting Vessel Voyage</td>
                        <td class="border_bottom">:</td>
                        <td class="border_bottom text_bold">@Model.Header.ConnectingVesselVoyage</td>
                        <td colspan="2" class="border_bottom border_right"></td>
                    </tr>
                    <tr>
                        <td colspan="3" class="border_left border_bottom">Final Destination</td>
                        <td class="border_bottom">:</td>
                        <td colspan="9" class="border_bottom border_right text_bold">@Model.Header.FinalDestination</td>
                    </tr>
                    <tr style="min-height:90px">
                        <td colspan="3" class="border_left border_bottom">Special Instruction</td>
                        <td class="border_bottom">:</td>
                        <td colspan="9" class="border_bottom border_right text_bold">@Model.Header.SpecialInstruction</td>
                    </tr>
                    <tr><td colspan="13" style="height:20px"></td></tr>
                    <tr>
                        <td colspan="6" style="vertical-align:top">
                            <table border="0" style="width:100%">
                                <tr><td class="tabledata_spacing text_bold">@Model.Header.StuffingDate</td></tr>
                                <tr><td class="tabledata_spacing text_bold">@Model.Header.StuffingDateOff</td></tr>
                                @foreach (var item in Model.ContainerTypes)
                                {
                                    <tr><td class="tabledata_spacing text_bold">@item</td></tr>
                                }
                                <tr><td class="tabledata_spacing text_bold">@Model.Header.Liner</td></tr>
                            </table>
                        </td>
                        <td colspan="7">
                            <table border="0" style="width:100%">
                                <thead>
                                    <tr>
                                        <th class="text_bold tabledata_spacing" rowspan="2" style="width:20%">CTNR #</th>
                                        <th class="text_bold tabledata_spacing" rowspan="2" style="width:60%">CTNR TYPE</th>
                                        <th class="text_bold tabledata_spacing" rowspan="2" style="width:20%">SEAL #</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.Detail)
                                    {
                                        <tr>
                                            <td class="tabledata_spacing text_left">@item.ContainerNumber</td>
                                            <td class="tabledata_spacing text_left">@item.ContainerType</td>
                                            <td class="tabledata_spacing text_right">@item.ContainerSealNumber</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td colspan="6" style="height:10px"></td>
                        <td colspan="7"></td>
                    </tr>
                    <tr>
                        <td colspan="5" style="height:30px"></td>
                        <td colspan="8"></td>
                    </tr>
                    <tr>
                        <td colspan="13">PREPARED BY :</td>
                    </tr>
                    <tr>
                        <td colspan="13">PT. Trakindo Utama - IMEX</td>
                    </tr>
                    <tr><td colspan="11">@ViewBag.TemplateHeader.SignedName</td></tr>
                    <tr><td colspan="11">@ViewBag.TemplateHeader.SignedPosition</td></tr>
                    <tr><td colspan="13" style="height:30px"></td></tr>
                    <tr>
                        <td colspan="13" class="text_footer">This is an electronic document, which printed and generated by computer integrated system. Required as reference for internal export document to get simple traceability. Weight & measurement are reference actual information as shown above.</td>
                    </tr>
                </table>
                <table class="table-document">
                    <tr>
                        <td><img src="@ViewBag.QrCodeUrlSI" height="150px;" width="150px;" /></td>
                    </tr>
                </table>
            </div>
        </div>
    </div>


</body>

</html>




