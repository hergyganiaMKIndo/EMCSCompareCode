@model  App.Web.Models.EMCS.CargoSsModel
@{
    ViewBag.Title = "DownloadSs";
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Document Download SHIPPING SUMMARY</title>
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro&display=swap" rel="stylesheet">
    <style type="text/css">
        html body {
            /*font-family: 'Arial Narrow', Arial, sans-serif;*/
            font-family: sans-serif;
            font-size: 11px;
            letter-spacing: 0.67ex;
            font-weight: 100 !important;
            line-height: 18px;
            text-rendering: optimizeSpeed;
        }

        table tr td, th {
            /*font-family: 'Arial Narrow', Arial, sans-serif;*/
            font-family: sans-serif;
            font-size: 11px;
            letter-spacing: 0.67ex;
            line-height: 18px;
            text-rendering: optimizeSpeed;
        }

        .hidden {
            display: none;
        }

        /* TABLE */
        .table thead tr {
            -webkit-box-shadow: 0px 6px 4px -3px rgba(222,222,222,0.72);
            -moz-box-shadow: 0px 6px 4px -3px rgba(222,222,222,0.72);
            box-shadow: 0px 6px 4px -3px rgba(222,222,222,0.72);
        }

            .table thead tr th {
                /*background: -moz-linear-gradient(left, rgba(60,141,188,1) 0%, rgba(60,141,188,0.93) 56%, rgba(61,142,190,0.93) 58%, rgba(86,156,235,0.93) 100%);
            background: -webkit-gradient(left top, right top, color-stop(0%, rgba(60,141,188,1)), color-stop(56%, rgba(60,141,188,0.93)), color-stop(58%, rgba(61,142,190,0.93)), color-stop(100%, rgba(86,156,235,0.93)));*/
                background-color: black;
                /* background: rgba(60,141,188,1); */
                font-weight: 600;
                line-height: 1;
                color: white;
            }

        .table tbody tr td {
            line-height: 1;
        }

        .table-document {
            border: none;
            border-collapse: collapse;
            width: 100%;
        }

            .table-document tr td {
                background: white;
            }

            .table-document tr td {
                line-height: 20px;
            }

        .table-custom {
            border: 0.5px solid darkgrey !important;
            width: 100%;
            border-collapse: collapse;
        }

        .table-data {
            border: 0.5px solid darkgrey !important;
            width: 100%;
            border-collapse: collapse !important;
        }

            .table-data tr td {
                /*border: 0.5px solid darkgrey !important;*/
            }

        .border_top {
            border-top: 0.5px solid darkgrey !important;
        }

        .border_bottom {
            border-bottom: 0.5px solid darkgrey !important;
        }

        .border_left {
            padding-left: 10px !important;
            border-left: 0.5px solid darkgrey !important;
        }

        .border_right {
            padding-right: 10px !important;
            border-right: 0.5px solid darkgrey !important;
        }

        .text_left {
            text-align: left !important;
        }

        .text_right {
            text-align: right !important;
        }

        .text_center {
            text-align: center !important;
        }

        .text_arial {
            font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
        }

        .text_bold {
            font-weight: 800;
        }

        .tabledata_spacing {
            padding-left: 10px;
            padding-right: 10px;
        }

        .text_docno {
            color: darkorange;
        }

        .text_gold {
            color: gold;
        }

        .text_docno_big {
            font-size: 15px;
            color: darkorange;
        }

        .text_20 {
            font-size: 20px !important;
        }

        .text_18 {
            font-size: 18px !important;
        }

        .text_16 {
            font-size: 16px !important;
        }

        .text_14 {
            font-size: 14px !important;
        }

        .text_12 {
            font-size: 12px !important;
        }

        .text_9 {
            font-size: 9px !important;
        }

        .text_8 {
            font-size: 8px !important;
        }

        .text_justify {
            text-align: justify;
        }

        .text_italic {
            font-style: italic;
        }

        .table-title-data {
            color: red;
        }

        .width_50_persen {
            width: 50%
        }

        .padding_left_10 {
            padding-left: 10px !important;
        }

        .padding_right_10 {
            padding-right: 10px !important;
        }

        thead {
            display: table-header-group
        }

        tfoot {
            display: table-row-group
        }

        tr {
            page-break-inside: avoid;
        }

        .table-title-data div {
            border-right: 1px solid #b9b4b4;
            border-bottom: 1px solid #b9b4b4;
            width: 300px;
            padding-left: 14px;
            margin-left: -10px;
            font-weight: 700;
            color: red;
            margin-top: 0px;
            line-height: 35px;
            color: black;
            z-index: 99999;
        }
    </style>
</head>
<body >
    <div style="width:950px !important;darkgray;padding:0px 0px 0px 0px;margin:20px auto">
        <div style="padding-left:4px;padding-right:4px">
            <div style="padding:10px">
                <table class="table-document">
                    <tr>
                        <td colspan="4" style="height:15px" class="border_left border_top border_right border_bottom text_bold">SHIPPER'S NAME & ADDRESS</td>
                        <td colspan="7" class="border_right"></td>
                    </tr>
                    <tr>
                        <td colspan="4" class="border_left text_bold">PT. TRAKINDO UTAMA</td>
                        <td colspan="3"></td>
                        <td colspan="4" class="border_right text_right text_bold">PREPARED BY :</td>
                    </tr>
                    <tr>
                        <td colspan="5" class="border_left">Gedung TMT 1, LT. 11 – 17 Suite 1101-1701,  Jl. Cilandak KKO No. 1</td>
                        <td colspan="2"></td>
                        <td colspan="4" style="text-align:right" class="border_right text_bold">PT. Trakindo Utama - IMEX</td>
                    </tr>
                    <tr>
                        <td colspan="5" class="border_left">Kel. Cilandak Timur, Kec. Pasar Minggu Jakarta Selatan </td>
                        <td colspan="2"></td>
                        <td colspan="4" class="border_right text_right">@Model.Header.RequestorName</td>
                    </tr>
                    <tr>
                        <td colspan="5" class="border_left">INDONESIA - 12560</td>
                        <td colspan="2"></td>
                        <td colspan="4" class="border_right text_right"><u>@Model.Header.RequestorEmail</u></td>
                    </tr>
                    <tr>
                        <td colspan="5" class="border_left border_bottom">Tel. (+62-21) 29976620 | Fax. (+62-21) 29976612 | www.trakindo.co.id</td>
                        <td colspan="6" class="border_bottom border_right"></td>
                    </tr>
                    <tr><td colspan="11" style="height:20px"></td></tr>
                    <tr>
                        <td colspan="4" style="height:15px" class="border_left border_top border_right border_bottom text_bold">CONSIGNEE'S NAME & ADDRESS</td>
                        <td colspan="2"></td>
                        <td colspan="4" class="border_left border_top border_right border_bottom text_bold">@((Model.Header.ShipDelivery == "Notify") ? "NOTIFY PARTY'S NAME & ADDRESS" : "SOLD TO'S NAME & ADDRESS")</td>
                        <td class="border_right"></td>
                    </tr>
                    <tr>
                        <td colspan="4"></td>
                        <td colspan="2"></td>
                        <td colspan="4"></td>
                        <td></td>
                    </tr>
                    <tr>
                        <td colspan="6" class="border_left text_bold">@Model.Header.ConsigneeName</td>
                        <td colspan="5" class="border_right text_bold">@((Model.Header.NotifyName == "-") ? Model.Header.SoldToName : Model.Header.NotifyName)</td>
                    </tr>
                    <tr>
                        <td colspan="6" class="border_left">@Model.Header.ConsigneeAddress</td>
                        <td colspan="5" class="border_right">@((Model.Header.NotifyName == "-") ? Model.Header.SoldToAddress : Model.Header.NotifyAddress)</td>
                    </tr>
                    <tr>
                        <td colspan="6" class="border_left">@Model.Header.ConsigneePic</td>
                        <td colspan="5" class="border_right">@((Model.Header.NotifyName == "-") ? Model.Header.SoldToPic : Model.Header.NotifyPic)</td>
                    </tr>
                    <tr>
                        <td colspan="6" class="border_left border_bottom">@Model.Header.ConsigneeEmail</td>
                        <td colspan="5" class="border_right border_bottom">@((Model.Header.NotifyName == "-") ? Model.Header.SoldToEmail : Model.Header.NotifyEmail)</td>
                    </tr>
                    <tr style="height:10px">
                        <td colspan="3"></td>
                        <td colspan="8"></td>
                    </tr>
                    <tr>
                        <td colspan="4"></td>
                        <td colspan="7"></td>
                    </tr>
                    <tr>
                        <td colspan="4" style="height:15px" class="border_left border_top border_right text_bold">CONTAINER CONTENTS LIST</td>
                        <td colspan="7"></td>
                    </tr>
                    <tr>
                        <td colspan="11">
                            <table border="0" class="table-data">
                                <thead>
                                    <tr>
                                        <th class="text_center text_bold border_bottom" style="height:2.5em;width:25%">DESCRIPTIONS</th>
                                        <th class="text_center text_bold border_bottom" style="width:15%">PACKAGES</th>
                                        <th class="text_center text_bold border_bottom" style="width:15%">VOLUME</th>
                                        <th class="text_center text_bold border_bottom" style="width:15%">NET WEIGHT</th>
                                        <th class="text_center text_bold border_bottom" style="width:15%">GROSS WEIGHT</th>
                                        <th class="text_center text_bold border_bottom" style="width:15%">TOTAL AMMOUNT</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td class="tabledata_spacing border_bottom">@Model.Header.Category</td>
                                        <td class="border_bottom"></td>
                                        <td class="border_bottom"></td>
                                        <td class="border_bottom"></td>
                                        <td class="border_bottom"></td>
                                        <td class="border_bottom"></td>
                                    </tr>
                                    @foreach (var item in Model.Detail)
                                    {
                                        <tr>
                                            <td class="tabledata_spacing">
                                                @item.CargoDescription<br />REFERENCE # : @item.CiplNo
                                            </td>
                                            <td class="tabledata_spacing text_right">@item.TotalCaseNumber</td>
                                            <td class="tabledata_spacing text_right">@item.TotalVolume</td>
                                            <td class="tabledata_spacing text_right">@item.TotalNetWeight</td>
                                            <td class="tabledata_spacing text_right">@item.TotalGrossWeight</td>
                                            <td class="tabledata_spacing text_right">@item.TotalAmount</td>
                                        </tr>
                                    }
                                    <tr class="spacing">
                                        <td colspan="6" class="tabledata_spacing"></td>
                                    </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>
                    <tr><td colspan="11" style="height:10px"></td></tr>
                    <tr>
                        <td colspan="7"></td>
                        <td colspan="2">TOTAL PACKAGES</td>
                        <td>:</td>
                        <td class="text_bold text_right">@Model.Header.TotalCaseNumber</td>
                    </tr>
                    <tr>
                        <td colspan="7"></td>
                        <td colspan="2">TOTAL NET WEIGHT (KGS)</td>
                        <td>:</td>
                        <td class="text_bold text_right">@Model.Header.TotalNetWeight</td>
                    </tr>
                    <tr>
                        <td colspan="7"></td>
                        <td colspan="2">TOTAL GROSS WEIGHT (KGS)</td>
                        <td>:</td>
                        <td class="text_bold text_right">@Model.Header.TotalGrossWeight</td>
                    </tr>
                    <tr>
                        <td colspan="7"></td>
                        <td colspan="2">TOTAL VOLUME (M3)</td>
                        <td>:</td>
                        <td class="text_bold text_right">@Model.Header.TotalVolume</td>
                    </tr>
                    <tr>
                        <td colspan="7"></td>
                        <td colspan="2">TOTAL AMOUNT</td>
                        <td>:</td>
                        <td class="text_bold text_right">@Model.Header.TotalAmount</td>
                    </tr>
                    <tr>
                        <td colspan="4" style="height:10px"></td>
                        <td colspan="7"></td>
                    </tr>
                    <tr>
                        <td colspan="4" style="height:15px" class="border_left border_top border_right text_bold">CARGO COLLECTED FROM </td>
                        <td colspan="7"></td>
                    </tr>
                    <tr>
                        <td colspan="3" style="height:15px" class="border_left border_top border_bottom">TRAKINDO</td>
                        <td colspan="8" class="border_top border_right border_bottom text_bold">: @Model.Header.Branch</td>
                    </tr>
                    <tr><td colspan="11" style="height:10px"></td></tr>
                    <tr><td colspan="11">Approved by,</td></tr>
                    <tr><td colspan="11">@ViewBag.TemplateHeader.SignedName</td></tr>
                    <tr><td colspan="11">@ViewBag.TemplateHeader.SignedPosition</td></tr>
                    <tr><td colspan="11" style="height:20px"></td></tr>
                    <tr><td colspan="11" class="text_footer">This is an electronic document, which printed and generated by computer integrated system. Required as reference for internal export document to get simple traceability. Weight & measurement are reference actual information as shown above.</td></tr>
                </table>
                <table class="table-document">
                    <tr>
                        <td><img src="@ViewBag.QrCodeUrlSS" height="150px" width="150px" /></td>
                    </tr>
                </table>
            </div>

        </div>
    </div>
</body>

</html>
@section Scripts{
    <script>
        var wizardState = @ViewBag.WizardData.Progress;
        var spacing = 0;
        if ('@Model.Detail.Count' < 25)
            spacing = ((250 / 20) - '@Model.Detail.Count') * 20;
        $('.spacing').css("height", spacing + 'px');
    </script>
    <script src="@Url.Content("~/Scripts.app/EMCS/Wizard/ProgressWizard.js")"></script>
    <script src="@Url.Content("~/Scripts.app/EMCS/Custom.js")"></script>
}