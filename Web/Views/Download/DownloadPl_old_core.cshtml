@model  App.Web.Models.EMCS.CiplModel
@{
    ViewBag.Title = "Download Packing List";
    Layout = null;
}

<table class="table-custom">
    <tr>
        <td colspan="3" class="padding_left_10 table-title-data border_left text_bold">
            <div class="border_bottom">Consignee</div>
        </td>
        <td colspan="3" class="table-title-data text_bold border_left" style="width:200px;">
            <div style="border-bottom:0.5px solid #c1b9b9;">Sold To/Ship To</div>
        </td>
    </tr>
    <tr>
        <td colspan="3" style="width:60%;" class="align_top padding_left_10 text_bold">@Model.TemplateHeader.ConsigneeName</td>
        <td colspan="3" style="width:40%;" class="align_top text_bold">@Model.TemplateHeader.NotifyName</td>
    </tr>
    <tr>
        <td colspan="3" class="padding_left_10 align_top">@Model.TemplateHeader.ConsigneeAddress</td>
        <td colspan="3" class=" align_top">@Model.TemplateHeader.NotifyAddress</td>
    </tr>
    <tr>
        <td class="padding_left_10 align_top" style="width:15%;">Telephone</td>
        <td class="align_top" style="width:2%;">:</td>
        <td class="align_top" style="width:43%;">@Model.TemplateHeader.ConsigneeTelephone</td>

        <td class="align_top" style="width:15%;">Telephone</td>
        <td class="align_top" style="width:2%;">:</td>
        <td class="align_top" style="width:23%;">@Model.TemplateHeader.NotifyTelephone</td>
    </tr>
    <tr>
        <td class="align_top padding_left_10">Fax</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.ConsigneeFax</td>

        <td class="align_top">Fax</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.NotifyFax</td>
    </tr>
    <tr>
        <td class="padding_left_10 align_top">Contact / Attn</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.ConsigneePic</td>

        <td class="align_top">Contact / Attn</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.NotifyPic</td>
    </tr>
    <tr>
        <td class="align_top padding_left_10">Email Address</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.ConsigneeEmail</td>

        <td class="align_top">Email Address</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.NotifyEmail</td>
    </tr>
</table>

<div>&nbsp;</div>
<table class="table-custom" style="width:200px;">
    <thead>
        <tr>
            <td class="border_left" style="width:200px;line-height:30px;border-left:1px solid black;">
                <div>
                    <strong>Description of Goods :</strong>
                </div>
            </td>
        </tr>
    </thead>
</table>
<table class="table-custom">
    <thead>
        <tr>
            <th class="text_center text_bold tabledata_spacing" rowspan="2" style="height:2.5em;width:6%">Case Number</th>
            <th class="text_center text_bold tabledata_spacing" rowspan="2" style="width:5%">Item No.</th>
            <th class="text_center text_bold tabledata_spacing" rowspan="2" style="width:10%">Name</th>
            <th class="text_center text_bold tabledata_spacing" rowspan="2" style="width:5%">Qty</th>
            <th class="text_center text_bold tabledata_spacing" rowspan="2" style="width:9%">Part Number</th>
            <th class="text_center text_bold tabledata_spacing" rowspan="2" style="width:9%">J-Code</th>
            <th class="text_center text_bold tabledata_spacing" rowspan="2" style="width:5%">CCR</th>
            <th class="text_center text_bold tabledata_spacing" rowspan="2" style="width:6%">Type</th>
            <th class="text_center text_bold tabledata_spacing" colspan="3" style="width:15%">Dimension (In CM)</th>
            <th class="text_center text_bold tabledata_spacing" rowspan="2" style="width:10%">VOLUME (M3)</th>
            <th class="text_center text_bold tabledata_spacing" rowspan="2" style="width:10%">Nett Weight (Kgs)</th>
            <th class="text_center text_bold tabledata_spacing" rowspan="2" style="width:10%">Gross Weight (Kgs)</th>
        </tr>
        <tr>
            <th class="text_center text_bold tabledata_spacing" style="width:5%;background-color:white">Length</th>
            <th class="text_center text_bold tabledata_spacing" style="width:5%;background-color:white">Width</th>
            <th class="text_center text_bold tabledata_spacing" style="width:5%;background-color:white">Height</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.TemplateDetail)
        {
            <tr>
                <td class="tabledata_spacing text_right">@item.CaseNumber</td>
                <td class="tabledata_spacing text_right">@item.ItemNo</td>
                <td class="tabledata_spacing">@item.Name</td>
                <td class="tabledata_spacing text_right">@item.Quantity</td>
                <td class="tabledata_spacing text_right">@item.PartNumber</td>
                <td class="tabledata_spacing">@item.JCode</td>
                <td class="tabledata_spacing">@item.Ccr</td>
                <td class="tabledata_spacing">@item.Type</td>
                <td class="tabledata_spacing text_right">@item.Length</td>
                <td class="tabledata_spacing text_right">@item.Width</td>
                <td class="tabledata_spacing text_right">@item.Height</td>
                <td class="tabledata_spacing text_right">@item.Volume</td>
                <td class="tabledata_spacing text_right">@item.NetWeight</td>
                <td class="tabledata_spacing text_right">@item.GrossWeight</td>
            </tr>
        }
    </tbody>
</table>

<div>&nbsp;</div>
<table class="table-custom">
    <tbody>
        <tr>
            <td class="border_top text_bold" style="width:20%;">TOTAL QUANTITY</td>
            <td class="border_top text_bold" style="width:3%;">:</td>
            <td class="border_top text_right" style="width:15%;">@Model.TemplateHeader.TotalQuantity</td>
            <td class="border_top" style="width:9%;">&nbsp;</td>
            <td class="border_top text_bold" style="width:20%;">TOTAL VOLUME (M3)</td>
            <td class="border_top text_bold" style="width:3%;">:</td>
            <td class="border_right border_top text_right" style="width:20%;">@Model.TemplateHeader.TotalVolume</td>
        </tr>
        <tr>
            <td class="text_bold">TOTAL COLLY</td>
            <td class="text_bold">:</td>
            <td class="text_right">@Model.TemplateHeader.TotalCaseNumber</td>
            <td></td>
            <td class="text_bold">TOTAL NET WEIGHT (Kgs)</td>
            <td class="text_bold">:</td>
            <td class="border_right text_right">@Model.TemplateHeader.TotalNetWeight</td>
        </tr>
        <tr>
            <td class="text_bold">&nbsp;</td>
            <td class="text_bold">&nbsp;</td>
            <td class="text_bold">&nbsp;</td>
            <td class="text_bold">&nbsp;</td>
            <td class="text_bold">TOTAL GROSS WEIGHT (Kgs)</td>
            <td class="text_bold">:</td>
            <td class="border_right text_right">@Model.TemplateHeader.TotalGrossWeight</td>
        </tr>
    </tbody>
</table>
<div>&nbsp;</div>

<table class="table-custom">
    <tr>
        <td class="width_50_persen table-title-data text_bold padding_left_10">
            <div>
                Shipping Marks & Nos :
            </div>
        </td>
        <td class="width_50_persen table-title-data text_bold border_left padding_left_10">
            <div>Remarks :</div>
        </td>
    </tr>
    <tr>
        <td class="border_left border_bottom border_right padding_left_10">@Model.TemplateHeader.ShippingMarksDesc</td>
        <td class="border_left border_bottom border_right padding_left_10">@Model.TemplateHeader.RemarksDesc</td>
    </tr>
    <tr>
        <td colspan="2" class="padding_left_10" style="height:10px">
            <div style="min-height:200px;">
                Approve By,
            </div>
        </td>
    </tr>

    <tr>
        <td colspan="2" class="padding_left_10" style="font-size:9px;"><i>Signature is not necessary due to this document is an electronic document which printed and generated by computer integrated system. Weight & measurement are reference information only and do not reflect the actual as shown above.</i></td>
    </tr>
</table>
