        <td colspan="4" class="border_right">@((Model.template_header.NotifyName == "-") ? Model.template_header.SoldToTelephone : Model.template_header.NotifyTelephone )</td>
    </tr>
    <tr>
        <td colspan="3" class="border_left">Fax</td>
        <td>:</td>
        <td colspan="3">@Model.template_header.ConsigneeFax</td>
        <td colspan="3"></td>
        <td colspan="2">Fax</td>
        <td>:</td>
        <td colspan="4" class="border_right">@((Model.template_header.NotifyName == "-") ? Model.template_header.SoldToFax : Model.template_header.NotifyFax )</td>
    </tr>
    <tr>
        <td colspan="3" class="border_left">Contact / Attn</td>
        <td>:</td>
        <td colspan="3">@Model.template_header.ConsigneePic</td>
        <td colspan="3"></td>
        <td colspan="2">Contact / Attn</td>
        <td>:</td>
        <td colspan="4" class="border_right">@((Model.template_header.NotifyName == "-") ? Model.template_header.SoldToPic : Model.template_header.NotifyPic )</td>
    </tr>
    <tr>
        <td colspan="3" class="border_left">Email Address</td>
        <td>:</td>
        <td colspan="3">@Model.template_header.ConsigneeEmail</td>
        <td colspan="3"></td>
        <td colspan="2">Email Address</td>
        <td>:</td>
        <td colspan="4" class="border_right">@((Model.template_header.NotifyName == "-") ? Model.template_header.SoldToEmail : Model.template_header.NotifyEmail )</td>
    </tr>
    <tr><td colspan="17" style="height:20px" class="border_left border_right"></td></tr>
</table>

<table class="table-custom">
