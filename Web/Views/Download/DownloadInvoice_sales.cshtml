    </tr>
    <tr>
        <td colspan="3" class="border_left">Telephone</td>
        <td>:</td>
        <td colspan="3">@Model.template_header.ConsigneeTelephone</td>
        <td colspan="3"></td>
        <td colspan="2">Telephone</td>
