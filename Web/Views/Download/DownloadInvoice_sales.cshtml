        <td colspan="2" class="text_bold">Shipment method:</td>
        <td class="text_bold">CoO:</td>
        <td class="text_bold" colspan="2">Vessel/Carrier:</td>
        <td colspan="3" class="text_bold">Sailing on or about:</td>
        <td colspan="3" class="text_bold">Load Port:</td>
        <td class="border_right text_bold">Destination Port:</td>
    </tr>
    <tr>
        <td class="border_left">@Model.template_header.CurrencyDesc</td>
        <td></td>
        <td colspan="3">@Model.template_header.ShipmentTerm</td>
        <td colspan="2">@Model.template_header.ShippingMethod</td>
        <td>@Model.template_header.CODesc</td>
        <td colspan="2">@Model.template_header.VesselCarier</td>
        <td colspan="3">@Model.template_header.SailingOn</td>
        <td colspan="3">@Model.template_header.LoadingPort</td>
        <td class="border_right">@Model.template_header.DestinationPort</td>
    </tr>
    <tr><td colspan="17" style="height:10px" class="border_left border_right"></td></tr>
    <tr>
        <td class="border_left text_bold" colspan="5">Payment Terms :</td>
        <td colspan="11" class="text_bold">Final Destination :</td>
        <td class="border_right text_bold">LC No & date :</td>
    </tr>
    <tr>
        <td class="border_left" colspan="5">@Model.template_header.PaymentTerms</td>
        <td colspan="11">@Model.template_header.FinalDestination</td>
        <td class="border_right">@Model.template_header.LcNoDate</td>
    </tr>
</table>

<div class="wrapper">&nbsp;</div>
<table class="table-custom" style="width:230px;">
    <thead>
        <tr>
            <td class="border_left text_bold" style="width:230px;line-height:22px;border-left:1px solid black;">
                Description of Goods :
            </td>
        </tr>
    </thead>
</table>

<table class="table-custom">
    <thead>
        <tr>
            <th class="text_center text_bold border_top border_bottom" style="width:10%">Case Number</th>
            <th class="text_center text_bold border_top border_bottom" style="text-align:center;width:10%;">Item No.</th>
            <th class="text_center text_bold border_top border_bottom" style="width:10%;">Name</th>
            <th class="text_center text_bold border_top border_bottom" style="width:8%;">Quantity</th>
            <th class="text_center text_bold border_top border_bottom" style="width:12%;">Part Number</th>
            <th class="text_center text_bold border_top border_bottom" style="width:10%;">J-Code</th>            
            <th class="text_center text_bold border_top border_bottom" style="width:10%;">Reference</th>            
            <th class="text_center text_bold border_top border_bottom" style="width:15%;">Unit Price</th>
            <th class="text_center text_bold border_top border_bottom" style="width:15%;">Extended Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.TemplateDetail)
        {
            <tr>
                <td class="tabledata_spacing text_12">@item.CaseNumber</td>
                <td class="tabledata_spacing text_12 text_center">@item.ItemNo</td>
                <td class="tabledata_spacing text_12">@item.Name</td>
                <td class="tabledata_spacing text_12 text_right">@item.Quantity</td>
                <td class="tabledata_spacing text_12">@item.PartNumber</td>
                <td class="tabledata_spacing text_12">@item.JCode</td>
                <td class="tabledata_spacing text_12">@item.ReferenceNo</td>                
                <td class="tabledata_spacing text_12 text_right">@item.UnitPrice</td>
                <td class="tabledata_spacing text_12 text_right">@item.ExtendedValue</td>
            </tr>
        }
        <tr style="height:@(Model.TemplateDetail.Count < 25 ? ((250 / 20) - Model.TemplateDetail.Count) * 20 : 0)px">
            <td colspan="9" class="tabledata_spacing"></td>
        </tr>
    </tbody>
</table>

