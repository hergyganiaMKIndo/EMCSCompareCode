@model  App.Web.Models.EMCS.CiplModel
<table class="table-custom">
    <tr>
        <td colspan="3" class="padding_left_10 table-title-data border_left text_bold">
            <div class="border_bottom">Consignee</div>
        </td>
        <td colspan="3" class="table-title-data text_bold border_left" style="width:200px;">
            <div style="border-bottom:0.5px solid #c1b9b9;">Sold To/Ship To</div>
        </td>
    </tr>
    <tr>
        <td colspan="3" style="width:60%;" class="align_top padding_left_10 text_bold">@Model.TemplateHeader.ConsigneeName</td>
        <td colspan="3" style="width:40%;" class="align_top text_bold">@Model.TemplateHeader.NotifyName</td>
    </tr>
    <tr>
        <td colspan="3" class="padding_left_10 align_top">@Model.TemplateHeader.ConsigneeAddress</td>
        <td colspan="3" class=" align_top">@Model.TemplateHeader.NotifyAddress</td>
    </tr>
    <tr>
        <td class="padding_left_10 align_top" style="width:15%;">Telephone</td>
        <td class="align_top" style="width:2%;">:</td>
        <td class="align_top" style="width:43%;">@Model.TemplateHeader.ConsigneeTelephone</td>

        <td class="align_top" style="width:15%;">Telephone</td>
        <td class="align_top" style="width:2%;">:</td>
        <td class="align_top" style="width:23%;">@Model.TemplateHeader.NotifyTelephone</td>
    </tr>
    <tr>
        <td class="align_top padding_left_10">Fax</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.ConsigneeFax</td>

        <td class="align_top">Fax</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.NotifyFax</td>
    </tr>
    <tr>
        <td class="padding_left_10 align_top">Contact / Attn</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.ConsigneePic</td>

        <td class="align_top">Contact / Attn</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.NotifyPic</td>
    </tr>
    <tr>
        <td class="align_top padding_left_10">Email Address</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.ConsigneeEmail</td>

        <td class="align_top">Email Address</td>
        <td class="align_top">:</td>
        <td class="align_top">@Model.TemplateHeader.NotifyEmail</td>
    </tr>
</table>

<table class="table-custom">
    <tr>
        <td style="vertical-align:top;" class="text_bold padding_left_10 text_center">Currency:</td>
        <td style="vertical-align:top;" class="text_bold text_center">Shipment Terms:</td>
        <td style="vertical-align:top;" class="text_bold text_center">Shipment method:</td>
        <td style="vertical-align:top;" class="text_bold text_center">CoO:</td>
        <td style="vertical-align:top;" class="text_bold text_center">Vessel/ Carrier:</td>
        <td style="vertical-align:top;" class="text_bold text_center">Sailing on or about:</td>
        <td style="vertical-align:top;" class="text_bold text_center">Load Port:</td>
        <td style="vertical-align:top;" class="text_bold text_center">Destination Port:</td>
    </tr>
    <tr>
        <td style="vertical-align:top;" class="text_12 padding_left_10">@Model.TemplateHeader.CurrencyDesc</td>
        <td style="vertical-align:top;" class="text_center text_12">@Model.TemplateHeader.ShipmentTerm</td>
        <td style="vertical-align:top;" class="text_center text_12">@Model.TemplateHeader.ShippingMethod</td>
        <td style="vertical-align:top;" class="text_center text_12">@Model.TemplateHeader.CoDesc</td>
        <td style="vertical-align:top;" class="text_center text_12">@Model.TemplateHeader.VesselCarier</td>
        <td style="vertical-align:top;" class="text_center text_12">@Model.TemplateHeader.SailingOn</td>
        <td style="vertical-align:top;" class="text_center text_12">@Model.TemplateHeader.LoadingPort</td>
        <td style="vertical-align:top;" class="text_center text_12">@Model.TemplateHeader.DestinationPort</td>
    </tr>
</table>

<div>&nbsp;</div>
<div class="wrapper">&nbsp;</div>
<table class="table-custom" style="width:200px;">
    <thead>
        <tr>
            <td class="border_left" style="width:200px;line-height:30px;border-left:1px solid black;">
                <div>
                    <strong>Description of Goods :</strong>
                </div>
            </td>
        </tr>
    </thead>
</table>

<table class="table-custom">
    <thead>
        <tr>
            <th class="text_center text_bold" style="height:2.5em;width:10%;border-bottom: 1px solid darkgrey;">Case Number</th>
            <th class="text_center text_bold" style="width:5%;border-bottom: 1px solid darkgrey;">Item No.</th>
            <th class="text_center text_bold" style="width:13%;border-bottom: 1px solid darkgrey;">Name</th>
            <th class="text_center text_bold" style="width:5%;border-bottom: 1px solid darkgrey;">Quantity</th>
            <th class="text_center text_bold" style="width:10%;border-bottom: 1px solid darkgrey;">Part Number</th>
            <th class="text_center text_bold" style="width:5%;border-bottom: 1px solid darkgrey;">J-Code</th>
            <th class="text_center text_bold" style="width:5%;border-bottom: 1px solid darkgrey;">CCR</th>
            <th class="text_center text_bold" style="width:5%;border-bottom: 1px solid darkgrey;">Type</th>
            <th class="text_center text_bold" style="width:15%;border-bottom: 1px solid darkgrey;">Unit Price</th>
            <th class="text_center text_bold" style="width:15%;border-bottom: 1px solid darkgrey;">Extended Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.TemplateDetail)
        {
            <tr>
                <td class="tabledata_spacing text_right">@item.CaseNumber</td>
                <td class="tabledata_spacing text_center">@item.ItemNo</td>
                <td class="tabledata_spacing">@item.Name</td>
                <td class="tabledata_spacing text_right">@item.Quantity</td>
                <td class="tabledata_spacing">@item.PartNumber</td>
                <td class="tabledata_spacing text_center">@item.JCode</td>
                <td class="tabledata_spacing">@item.Ccr</td>
                <td class="tabledata_spacing">@item.Type</td>
                <td class="tabledata_spacing text_right">@item.UnitPrice</td>
                <td class="tabledata_spacing text_right">@item.ExtendedValue</td>
            </tr>
        }
    </tbody>
</table>
<div>&nbsp;</div>

<table class="table-custom">
    <tbody>
        <tr>
            <td class="border_top text_bold padding_left_10" style="width:20%;">TOTAL QUANTITY</td>
            <td class="border_top text_bold" style="width:3%;">:</td>
            <td class="border_top text_right" style="width:15%;">@Model.TemplateHeader.TotalCaseNumber</td>

            <td class="border_top" style="width:9%;">&nbsp;</td>

            <td class="border_top text_bold" style="width:20%;">&nbsp;</td>
            <td class="border_top text_bold" style="width:3%;">&nbsp;</td>
            <td class="border_top text_right" style="width:15%;">&nbsp;</td>
        </tr>
        <tr>
            <td class="border_top text_bold padding_left_10" style="width:20%;">TOTAL COLLY</td>
            <td class="border_top text_bold" style="width:3%;">:</td>
            <td class="border_top text_right" style="width:20%;">@Model.TemplateHeader.TotalCaseNumber</td>

            <td class="border_top" style="width:9%;">&nbsp;</td>

            <td class="border_top text_bold" style="width:20%;">TOTAL VALUE</td>
            <td class="border_top text_bold" style="width:3%;">:</td>
            <td class="border_right border_top text_right" style="width:20%;">@Model.TemplateHeader.TotalExtendedValue</td>
        </tr>
    </tbody>
</table>

<div class="wrapper">&nbsp;</div>
<table class="table-custom">
    <tr>
        <td class="width_50_persen table-title-data text_bold padding_left_10">
            <div>
                Shipping Marks & Nos :
            </div>
        </td>
        <td class="width_50_persen table-title-data text_bold border_left padding_left_10">
            <div>Remarks :</div>
        </td>
    </tr>
    <tr>
        <td class="border_left border_bottom border_right padding_left_10">@Model.TemplateHeader.ShippingMarksDesc</td>
        <td class="border_left border_bottom border_right padding_left_10">@Model.TemplateHeader.RemarksDesc</td>
    </tr>
    <tr>
        <td colspan="2" class="padding_left_10" style="height:10px">
            <div style="min-height:100px;">
                Approve By,
            </div>
        </td>
    </tr>
    <tr>
        <td colspan="2" class="text_justify text_12 text_italic padding_left_10 padding_right_10 text_9">
            <i>Signature is not necessary due to this document is an electronic document which printed and generated by computer integrated system. I/We hereby certify that the information on this invoice is true and correct, and that the contents of this shipment as stated above.</i>
        </td>
    </tr>
</table>

