@model  App.Web.Models.EMCS.CiplModel
<!DOCTYPE html>
<html>
<head>
    <script>
        function subst() {
            var vars = {};
            var x = document.location.search.substring(1).split('&');
            for (var i in x) { var z = x[i].split('=', 2); vars[z[0]] = unescape(z[1]); }
            var x = ['frompage', 'topage', 'page', 'webpage', 'section', 'subsection', 'subsubsection'];

            var n = document.getElementById("header_document");
            var m = document.getElementsByClassName("address_document");

            for (var i in x) {
                var y = document.getElementsByClassName(x[i]);
                for (var j = 0; j < y.length; ++j) {
                    y[j].textContent = vars[x[i]];
                    if (vars['page'] != 1) {
                        for (var h = 0; h < m.length; ++h) {
                            m[h].innerHTML = "";
                        }
                    }
                }
            }
        }
    </script>
    <link href="../../Content/EMCS/DocumentDownloadHeader.css" rel="stylesheet" />
    <style>
        html body {
            font-family: sans-serif;
            font-size: 10px;
            letter-spacing: 0.65ex;
            font-weight: 100 !important;
            line-height: 18px;
            text-rendering: optimizeSpeed;
        }

        table tr td, th {
            font-family: sans-serif;
            font-size: 10px;
            letter-spacing: 0.65ex;
            line-height: 18px;
            text-rendering: optimizeSpeed;
        }

        .border_top {
            border-top: 1px solid darkgrey !important;
        }

        .border_bottom {
            border-bottom: 1px solid darkgrey !important;
        }

        .border_left {
            padding-left: 10px !important;
            border-left: 1px solid darkgrey !important;
        }

        .border_right {
            padding-right: 10px !important;
            border-right: 1px solid darkgrey !important;
        }
    </style>
</head>
<body id="header_document" style="margin: 0 auto; padding-left: 31px; padding-right: 23px;" onload="subst()">
    <table class='table_grid' id="InvSales0" style="width:100%;border-collapse:collapse;">
        <tr>
            <td colspan="6">
                <img src="../../Images/trakindo_logo.jpg" />
            </td>
            <td colspan="6" class="border_left border_top border_right border_bottom text_bold header_title" style="font-size:14.4px">PACKING LIST</td>
        </tr>
        <tr>
            <td style="width:140px;">&nbsp;</td>
            <td colspan="5" class="text_bold text_14">PT. TRAKINDO UTAMA</td>
            <td colspan="2" class="border_left">Number</td>
            <td>:</td>
            <td colspan="3" class="border_right text_bold text_docno_big">@Model.TemplateHeader.CiplNo</td>
        </tr>
        <tr>
            <td></td>
            <td colspan="5">Gedung TMT 1, LT. 11&minus;17 Suite 1101-1701,  Jl. Cilandak KKO No. 1</td>
            <td colspan="2" class="border_left">Date</td>
            <td>:</td>
            <td colspan="3" class="border_right">@Model.Data.UpdateDate</td>
        </tr>
        <tr>
            <td></td>
            <td colspan="5">Kel. Cilandak Timur, Kec. Pasar Minggu Jakarta Selatan</td>
            <td colspan="2" class="border_left">For</td>
            <td>:</td>
            <td colspan="3" class="border_right">@Model.TemplateHeader.Area</td>
        </tr>
        <tr>
            <td></td>
            <td colspan="5">INDONESIA - 12560</td>
            <td colspan="2" class="border_left">Contact / Attn</td>
            <td>:</td>
            <td colspan="3" class="border_right">@Model.TemplateHeader.RequestorName</td>
        </tr>
        <tr>
            <td></td>
            <td>Tel.</td>
            <td>:</td>
            <td colspan="3">(62-21) 29976620</td>
            <td colspan="2" class="border_left">Email Address</td>
            <td style="text-align:right;">:</td>
            <td colspan="3" class="border_right">@Model.TemplateHeader.RequestorEmail</td>
        </tr>
        <tr>
            <td></td>
            <td>Fax.</td>
            <td>:</td>
            <td colspan="3">(62-21) 29976612</td>
            <td colspan="2" class="border_left border_bottom">Category</td>
            <td class="border_bottom" style="text-align:right;">:</td>
            <td colspan="3" class="border_bottom border_right">@Model.Data.Category - @Model.Data.CategoriItem</td>
        </tr>
        <tr>
            <td colspan="12" class="text_right">[Page <span class="page"></span> of <span class="topage"></span> ]</td>
        </tr>
    </table>
</body>
</html>

